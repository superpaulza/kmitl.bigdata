        <div class="story-content">

                            <script>
                    var lead_carousel = [{"cloudinaryId":"aztzrfqmpnzeg76yonag","format":"jpg","transformations":null,"width":680,"height":440,"type":"Image","caption":"Henry Davis, 2009, image courtesy of <a href=\"http:\/\/www.thedailybeast.com\/articles\/2014\/08\/15\/the-day-ferguson-cops-were-caught-in-a-bloody-lie.html\" target=\"_blank\">The Daily Beast<\/a>","altText":"Ferguson Police Beating","id":1290746,"dateDeleted":null,"dateCreated":1408123487000,"dateUpdated":null,"class":"Image"}];
