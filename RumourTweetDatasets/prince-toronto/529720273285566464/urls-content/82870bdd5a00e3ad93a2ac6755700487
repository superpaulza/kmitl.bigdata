                          x.parentNode.insertBefore(s, x);
                      }
                      loadHorizon();
                      var oldOnLoad = window.onload;
                      window.onload = function() {
                          if (typeof oldOnLoad === 'function') {
                              oldOnLoad();
                          }
                          if (typeof Sailthru !== 'undefined') {
                          Sailthru.setup({
        domain: 'horizon.billboard.com',
      }); 
                          }
                      };

        
//--><!]]>
</script>
