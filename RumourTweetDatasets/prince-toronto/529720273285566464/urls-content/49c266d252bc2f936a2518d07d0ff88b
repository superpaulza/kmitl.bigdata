<script type="text/javascript">
$j(function(){
	function setWallpaperCss(){
		var iFooterBottomExtra = 46;
		var iHeight = ($j("body").height() - $j("#midwrapper").offset().top) + iFooterBottomExtra;
		$j("#midwrapper .wallpaperad")
		.css({
			height: iHeight,
			cursor: "pointer"		
		})
		.click(function(){
			var url = "#";
			
			if(url != '' && url != '#'){
				window.location = url;
			}
		});
	}
	
	setWallpaperCss();
	
	$j(window).load(function(){
		setWallpaperCss();
	});
});
</script>
