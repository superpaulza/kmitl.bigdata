		<div id="divLogOut-7_489985_1423585512_819957172" style="display:none;" class="tve-button-logout"><a href="#" onclick="axisAuthApi.performLogout('logout'); Tracking.trackLogout(); return false;">Video Sign Out</a></div>
	</div>
	<script type="text/javascript">
	requiresDependency('http://cauth.9c9media.com/auth9/axisAuthApi.js', function() {
  	axisAuthApi.events.addEvent('on_authentication_state_change', function (authenticationStatus, options) {
  		if (authenticationStatus == true) { 
  			$('#divLogIn-7_489985_1423585512_819957172').hide(); $('#divLogOut-7_489985_1423585512_819957172').show(); 
			}
  		else { 
				$('#divLogIn-7_489985_1423585512_819957172').show(); $('#divLogOut-7_489985_1423585512_819957172').hide(); 
			}
  	});
		});
				function jump(h){
    var top = document.getElementById(h).offsetTop;
    window.scrollTo(0, top);
}
	</script>
