  var mySlider;
  jQuery(document).ready(function() {
    requiresDependency('http://static.ctvnews.ca/bellmedia/common/js/jquery.bxslider.min.js', function() {
        mySlider = $('#rotator_819954023_7_513024').bxSlider({
                      
                      
                      
                      
                                            minSlides: 1,
                      slideWidth:145,
                      //onSlideNext : function($slideElement, oldIndex, newIndex){
                      //    getCarouselSlide("7.513024", "rotator_819954023_7_513024", newIndex+1);
                          //alert('onSlideNext. oldIndex=[' + oldIndex + '], newIndex=[' + newIndex + '], slideWidth=[6]');              
                      //},
                      //onSlidePrev : function($slideElement, oldIndex, newIndex){
                      //    getCarouselSlide("7.513024", "rotator_819954023_7_513024", newIndex+1);
                          //alert('onSlidePrev. oldIndex=[' + oldIndex + '], newIndex=[' + newIndex + '], slideWidth=[6]');              
                      //},
                      onSlideBefore : function ( $slideElement, oldIndex, newIndex ) {
                        getCarouselSlide7_513024_819954023("7.513024", "rotator_819954023_7_513024", newIndex+1, "7_513024_819954023");
                      },
                      moveSlides: 6,
                      infiniteLoop:false,hideControlOnEnd: true,
                      maxSlides: 6,
                                              prevText: 'Previous'
                                          });
        
        $('#rotator_819954023_7_513024').css("visibility", "visible");
              });
      
      
      // get first slide
      //getCarouselSlide7_513024_819954023("7.513024", "rotator_819954023_7_513024", 1, "7_513024_819954023");
  });
  
  
  function getCarouselSlide7_513024_819954023(path, containingDiv, pageNum, carouselUniqueId) {
  
    if ( loadedPageNum7_513024_819954023.indexOf(pageNum) != -1 ){
      // slide already loaded, no need to do anything
    } else {
      // slide not loaded yet, load it.
        if (path != null) {
          
          //var carouselId = path.replace(/\./g, "_");
          
          var maxItemsPerPage = 6;
          var startIndex = (maxItemsPerPage * pageNum) - maxItemsPerPage;
          var endIndex = startIndex + maxItemsPerPage;
          
          if ( endIndex > 36 ) {
            endIndex = 36;
          }
          
          // need to specify the carousel parent site contentID to correctly render video link with dedicated video page
          var targetPath = "2.620/" + path + "?ot=example.AjaxPageLayout.ot";
          
          if (pageNum != null) {
            targetPath = targetPath + "&pageNum=" + pageNum;
          }
      
          // unique carousel id
          targetPath = targetPath + "&carouselUniqueId=7_513024_819954023";
          
          // teaser image deriv and img width
          targetPath = targetPath + "&imgDeriv=landscape";
          targetPath = targetPath + "&imgWidth=150";
          targetPath = targetPath + "&teaserHeadlineType=normal";
          
          targetPath = targetPath + "&isAjax=true";
      
          jQuery.ajax({
            url : targetPath,
            success : function(data) {
              
              //alert(data);  
              //alert('done ajax: carouselId=' + carouselId + ', startIndex=' + startIndex + ', endIndex=' + endIndex);
              
              if ( (endIndex - startIndex) < maxItemsPerPage ) {
                 startIndex = endIndex - maxItemsPerPage;
                 if ( startIndex < 0 ) {
                  startIndex = 0;
                 }
              }
              
              for ( k = startIndex; k < endIndex ; k ++ ){
                var carouselIndex = k + 1; // carousel index is 1-based
                //alert('carouselINdex=['+carouselIndex+']');
                //alert($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                
                //$("#carousel" + carouselId + "_"+carouselIndex).html($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                $("#carousel7_513024_819954023_"+carouselIndex).html($(data).find("#carousel7_513024_819954023_"+carouselIndex).html());
                
              }
              
              // flag this page has been loaded
              loadedPageNum7_513024_819954023.push(pageNum);
              
            }
          });
        }
    }
  }
  
  // keep track of which carousel page has been loaded.  If loaded, no need to load again.
  var loadedPageNum7_513024_819954023 = new Array();
  </script>


  

  </div>
  
                      </div><!-- END .col-wrapper -->
        </div>
    </div>
    
  
