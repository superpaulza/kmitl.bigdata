	<div 
			class="container module"
		>
		<div class="grid-24 in-module" id="more-progress" style="display:none;">
			<img src="http://d2x3wmakafwqf5.cloudfront.net/application/theme/images/loaders/facebook.gif" alt="loading" />
		</div>
		<div class="grid-24 in-module more-last-played">
			<a class="btn btn-black more-last-played" id="more-last-played" href="">More</a>
		</div>
	</div>
	
	<script type="text/javascript">
		var numSongsPerPage = 5;
		var moreClicks = 1;
		//$(document).ready(function(){$(".container.module.more").hide();});
		$("#more-last-played").click(function(e){
			e.preventDefault();
			$("#more-progress").show();
			moreClicks++;
			updateLastPlayed();
		});
		updateLastPlayed = function(){
			var numSongs;
			numSongs = numSongsPerPage * moreClicks + 1;
			$.ajax({
				type: "GET",
				dataType: 'html',
				url: "/community/getLastPlayedSongs",
				data:{ "num": numSongs},
				success: function(response){
					$("#last-played").html(response);
				},
				error: function(){
					//console.log("Oops");
				},
				complete: function(){
					$("#more-progress").hide();
				}
			});
		}
	</script>

