<script type="text/javascript">
var fullVolume = 56;
var socast_volume = 100;
var last_volume = 100;
setVolume = function(vol){
	socast_volume = vol;
	if (socast_volume == 0){
		$("#volume img").attr('src','http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/modules/player/images/SoCast_CMS-Player_Controls-FixedSize_21x18_MuteIcon.png');
	}else{
		$("#volume img").attr('src','http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/modules/player/images/SoCast_CMS-Player_Controls-FixedSize_21x18_SoundIcon.png');
	}
	setPlayerVolume(socast_volume);
}
function getOffset(el) {
    if (el.getBoundingClientRect)
        return el.getBoundingClientRect();
    else {
        var x = 0, y = 0;
        do {
            x += el.offsetLeft - el.scrollLeft;
            y += el.offsetTop - el.scrollTop;
        } 
        while (el = el.offsetParent);

        return { "left": x, "top": y }
    }      
}
$(document).ready(function(){
	
	initialVolume = function(vol){
		var y = fullVolume - vol * fullVolume /100;
		$("#vol-blank img").attr('src','http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/modules/player/images/blank5.png');
		$("#vol-blank").css('clip','rect(0px,40px,'+y+'px,0px)');
		setVolume(vol);
	}
	try {
		$("#volume-panel").hide();
		$("#volume").mouseover(function(){
			$("#volume-panel").show();
		});
		$("#volume").mouseleave(function(){
			$("#volume-panel").hide();
		});
		$("#volume-panel").mouseover(function(){
			$("#volume-panel").show();
		});
		$("#volume-panel").mouseleave(function(){
			$("#volume-panel").hide();
		});
		$(".control-button").live('click',function(){
			var buttonId = $(this).attr('id');
			switch (buttonId){
			case 'play':
				startPlay();
				break;
			case 'stop':
			case 'pause':
				pausePlay();
				break;
			case 'volume':
				if (socast_volume == 0){ //unmute
					if (last_volume < 30) last_volume = 30;
					initialVolume(last_volume);
				}else{ //mute
					last_volume = socast_volume;
					socast_volume = 0;
					initialVolume(socast_volume);
				}
				break;
			case 'zoom':
				break;
			}
		});
		$("div[id^='vol-']").click(function(e){
			/*
				volume height is 57px, top offset is 11px
				y-objBlank.top is the volume, mapping like: 0 = 100 ,  56 = 0
			*/
			var y=e.clientY;
			var objBlank;
			objBlank = getOffset(document.getElementById('vol-blank'));
			y = y - objBlank.top ;
			if (y < 0) y = 0;
			else if (y > fullVolume) y = fullVolume;
			/*
			var xx = e.clientX;
			var yy = e.clientY;
			//console.log("xx yy "+xx + ","+ yy);
			//console.log("top left "+ obj.top + ","+ obj.left);
			*/
			if ($(this).attr('id') == "vol-green"){
				$("#vol-blank img").attr('src','http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/modules/player/images/blank5.png');
			}else{
				//console.log('not green');
			}
			$("#vol-blank").css('clip','rect(0px,40px,'+y+'px,0px)');
			setVolume((fullVolume-y)*100/fullVolume);
			return false;// fixed volume issue. prevent cookie from being 0, don't know why
		});
		if (window.top != window.self){
			if (parent.socast_player_vol < 0) {
				socast_volume = getCookie('socast_player_playerCorus_volume');
				if (socast_volume == false || socast_volume == 30) socast_volume = 100;
				if (socast_volume < 30) socast_volume = 30;
				if (!isMobileUserAgent){// jplayer(for mobile) volume from 0 to 1
					socast_volume = socast_volume / 100; // desktop uses JWplayer, volume is from 1 to 100
				}
				initialVolume(socast_volume * 100);
							} else {
				if (parent.socast_player_vol >0 && parent.socast_player_vol <=1) initialVolume(parent.socast_player_vol*100);
				else initialVolume(parent.socast_player_vol);
			}
		}
		setTimeout(function(){
			var status = getPlayerStatus();
			$("#play,#pause,#stop").attr('id',status);
			$("#"+status).trigger('click');
		},2000);
	} catch (e) {
		//if (!isIE()) console.log('Error setting up player controls',e);
	}
	
});
</script>

