<div id='modal_player_overlay_background'>
	<div id="modal_player_overlay">
		<script type="text/javascript">
///////////////////////////////////////////////////////////////////////////
// Pre-roll ad code handling
///////////////////////////////////////////////////////////////////////////
var callLetters = "CILQFM";
var adPreRoll = null;
var hasFlash = false;
var duration = 20;
var ad_duration = 15;
var ad_countdown = 15;

// return a video ad object given json data from the ad server
function getVideoAd(data) {
	try {
		// get third party tracker
		if (data.results[0].mediafiles.mediafile[0].thirdPartyTracker && data.results[0].mediafiles.mediafile[0].thirdPartyTracker != "[impression]") {
			$(document).ready(function() {
				$("body").append('<img src="'+data.results[0].mediafiles.mediafile[0].thirdPartyTracker+'" alt="tracker" />');
			});
		}

		// get video or audio ad
		switch (data.results[0].mediafiles.mediafile[0].type) {
			case "video/x-flv":
				return {
					type: "video",
					creative: data.results[0].mediafiles.mediafile[0].url,
					audioAdImage: '',
					clickThrough: data.results[0].mediafiles.mediafile[0].clickthrough,
					duration: data.results[0].mediafiles.mediafile[0].duration
				};
				break;
			case "audio":
				return {
					type: "audio",
					creative: data.results[0].mediafiles.mediafile[0].url,
					audioAdImage: data.results[0].companionads.companionad[0].url,
					clickThrough: data.results[0].mediafiles.mediafile[0].clickthrough,
					duration: data.results[0].mediafiles.mediafile[0].duration
				};
				break;
		}
		return null;
	} catch(e) { 
		//if (this.console) console.log("Error loading video ad", e);
		return null;
	}
}

function getCompanionAdBar(data) {
	try {
		if (data.results[0].companionads.companionad300x50 !== undefined) {
			var adObj = {};
			if (data.results[0].companionads.companionad300x50[0].clickthrough !== undefined) {
				adObj.clickthrough = data.results[0].companionads.companionad300x50[0].clickthrough;
			} else {
				adObj.clickthrough = '';
			}
			
			if (data.results[0].companionads.companionad300x50[0].url !== undefined) {
				adObj.image = data.results[0].companionads.companionad300x50[0].url;
			} else {
				adObj.image = '';
			}
			
			if (adObj.image != '') return adObj;
			else return null;
		}
	} catch(e) { 
		return null;
	}
}

function showCompanionAdBar() {
	if (parent.companionAd != null) {
		var html = "<a href='"+parent.companionAd.clickthrough+"' target='_blank'><img src='"+parent.companionAd.image+"' style='height:50px;width:300px;' alt='companion' /></a>";
		if (parent.companionAd.image != '') $('#player_wrapper_click').html(html);
	}
}

function getCompanionAdWrapper(data) {
	//return {"clickthrough":"#","image":"http://admin.socastsrm.com/images/wrapper.png"};
	try {
		if (data.results[0].companionads.companionad320x500 !== undefined) {
			var adObj = {};
			if (data.results[0].companionads.companionad320x500[0].clickthrough !== undefined) {
				adObj.clickthrough = data.results[0].companionads.companionad320x500[0].clickthrough;
			} else {
				adObj.clickthrough = '#';
			}
			
			if (data.results[0].companionads.companionad320x500[0].url !== undefined) {
				adObj.image = data.results[0].companionads.companionad320x500[0].url;
			} else {
				adObj.image = '';
			}
			
			if (adObj.image != '') return adObj;
			else return null;
		}
		if (data.results[0].companionads.companionad320x400 !== undefined) {
			var adObj = {};
			if (data.results[0].companionads.companionad320x400[0].clickthrough !== undefined) {
				adObj.clickthrough = data.results[0].companionads.companionad320x400[0].clickthrough;
			} else {
				adObj.clickthrough = '#';
			}
			
			if (data.results[0].companionads.companionad320x400[0].url !== undefined) {
				adObj.image = data.results[0].companionads.companionad320x400[0].url;
			} else {
				adObj.image = '';
			}
			
			if (adObj.image != '') return adObj;
			else return null;
		}
		if (data.results[0].companionads.companionad320x260 !== undefined) {
			var adObj = {};
			if (data.results[0].companionads.companionad320x260[0].clickthrough !== undefined) {
				adObj.clickthrough = data.results[0].companionads.companionad320x200[0].clickthrough;
			} else {
				adObj.clickthrough = '#';
			}
			
			if (data.results[0].companionads.companionad320x260[0].url !== undefined) {
				adObj.image = data.results[0].companionads.companionad320x200[0].url;
			} else {
				adObj.image = '';
			}
			
			if (adObj.image != '') return adObj;
			else return null;
		}
	} catch(e) { 
		return null;
	}
}

function showCompanionAdWrapper() {
	if (parent.companionAdWrapper != null) {
		$('.player-wrapper-bottom').show();
		var height = 400;
		if (playerType == 'program') height = 260;
		$('#player-wrapper').css('height',height+'px');
		$('.player-wrapper-bottom').css('top',(height-10)+'px');
		var html = "<img src='"+parent.companionAdWrapper.image+"' style='height:"+height+"px;width:320px;' alt='companion' />";
		if (parent.companionAdWrapper.image != '') $('#player-wrapper').html(html);
	}
}

function initPrerollPlayer(alreadyLoaded) {
	closeYoutube();
	if (alreadyLoaded == undefined) alreadyLoaded = false;
	// play a d here is safe
	if (!hasFlash || adPreRoll == null) {
		//if (!isIE()) console.log('preroll player: no flash or no ad check');
		$('#player_wrapper_click').hide();
		closeModal(true);
		return;
	}
	$("#expanded-player").show();
	/*if (alreadyLoaded) {
		jwplayer("jquery_jplayer_preroll").remove();
	}*/
	
	var preroll_player_height = 200;
	var preroll_player_width = 300;
	
	if (preroll_type == 'modal') {
		$('#modal_overlay').show();
		$('#modal_player_overlay_background').show();
		preroll_player_height = 300;
		preroll_player_width = 480;
	} else {
		//$("#expanded-player").show();
		$("#radio-player").hide();
	}
	
	
	//var jplayer_preroll_supplied = 'flv';
	
	if (hasFlash && adPreRoll && adPreRoll.type == "video") {
		//jplayer_preroll_supplied = 'flv';
		if (preroll_type == 'player') $('#modal_player_overlay').css('height','215px');
		$('#jp_container_preroll').click(function() {
			if (adPreRoll != undefined && adPreRoll && adPreRoll.clickThrough != undefined && adPreRoll.clickThrough) {
				window.open(adPreRoll.clickThrough);
			}
		});
	} else if (hasFlash && adPreRoll && adPreRoll.type == "audio") {
		//jplayer_preroll_supplied = 'mp3';
		if (preroll_type == 'player') $('#modal_player_overlay').css('height','265px');
		else $('#jquery_jplayer_preroll_image').css('top','20px');
	}
	
	var preroll_source = {file: adPreRoll.creative, type: 'flv'};
	if (adPreRoll.type == 'audio') preroll_source.type = 'mp3';
	//var ie8iframe = "<iframe width='"+preroll_player_width+"' height='"+preroll_player_height+"' src='/player/playerCorusAdFrame' scrolling='no'></iframe>";
	//$('#jquery_jplayer_preroll').html(ie8iframe);
	jwplayer("jquery_jplayer_preroll").setup({
		height: preroll_player_height,
		width: preroll_player_width,
		controls: false,
		//wmode: 'transparent',
		primary: "flash",
		//file: adPreRoll.creative,
		playlist:[{sources: [preroll_source]}],
		bufferLength: 15, 
		onBufferEmpty: function () {
			var ct = this.getTime();
			if (ct > 1) {
				this.seek(ct - 0.5);
			}
		},
		events: {
			onTime: checkTime,
			onError: function(e) {
				closeModal(true);
			},
			onSetupError: function(e) {
				closeModal(true);
			},
			onPause: function(e) {
				window.open(adPreRoll.clickThrough);
				closeModal(true);
			},
			onReady: function(e) {
				$("#listen-live img").attr('src',PLAYER_ASSETS+"SoCast_CMS-Player_ListenLiveBtn-Active_134x35.png");
				$("#listen-live img").attr('data-state','pause');
				$('#now-playing-over-wrapper div.in-module.now-playing').append("<div id='listenLiveBtnBlocker' style='width:145px;height:40px;position:absolute;right:9px;top:0px;text-align:right;'><img id='listen-live-image' alt='listen live' data-state='pause' src='"+PLAYER_ASSETS+"SoCast_CMS-Player_ListenLiveBtn-Active_134x35.png' style='margin-top:1px;'></div>");
								setPrerollPlayerVolume(100);
				if (hasFlash && adPreRoll && adPreRoll.type == "audio") {
					$("#jquery_jplayer_preroll_image").html('<a id="preroll-audio-link" href="'+adPreRoll.clickThrough+'" target="_blank"><img id="preroll-audio-image" src="'+adPreRoll.audioAdImage+'" alt="'+adPreRoll.audioAdImage+'" /></a>');
				}
				this.play();
			}
		}
	});
		
	showCompanionAdBar();
	showCompanionAdWrapper();
}

function showPreroll() {
	//$("#jquery_jplayer_preroll").jPlayer('play');
	jwplayer("jquery_jplayer_preroll").play();
}

function showPrerollHack() {
	showPreroll(); //init player
}

var checkModalTimeout = null;
function checkTime(data) {
	ad_duration = Math.floor(data.duration);
	ad_countdown = ad_duration - Math.floor(data.position);
	if (ad_countdown <= 0) {
		//jwplayer("jquery_jplayer_preroll").pause();
		closeModal(true);
	} else {
		if (ad_countdown == 1) {
			clearTimeout(checkModalTimeout);
			checkModalTimeout = setTimeout(checkCloseModal,2000);
		}
		setPrerollTimeLeft(ad_countdown);
	}
}

function setPrerollTimeLeft(num) {
	$('#second-left').html(num);
}

function checkCloseModal() {
	if ($('#modal_player_overlay_background').is(':visible')) {
		closeModal(true);
	}
}

function setPrerollPlayerVolume(num) {
	jwplayer("jquery_jplayer_preroll").setVolume(num);
}
	
function initPreroll(alreadyLoaded) {
	pausePlay();
	if (alreadyLoaded == undefined) alreadyLoaded = false;
	//$("#expanded-player").show();
	var artistImg_cookie = getCookie('socast_player_artist_image');
	if (artistImg_cookie) $("#artist-gallery").append(artistImg_cookie);
	$("#listen-live img").attr('src',PLAYER_ASSETS+"SoCast_CMS-Player_ListenLiveBtn-Active_134x35.png");
	$("#listen-live img").attr('data-state','pause');
	// load video a d
	var adurl = "http://adserver.adtechus.com/?adrawdata|3.0|5233.1|1203131|0|-1|ADTECH;alias=cilqfm2_REPLACEWITHPAGEALIAS_1x1;size=1x1;key=socastjson;loc=100;noperf=1;target=_blank;grp=13333;misc=2085171544";
	//adurl = "http://pubads.g.doubleclick.net/gampad/adx?iu=/23171097/Radio_test/TEST_RM_TEMPLATES&sz=640x480&m=application/x-javascript&t=url%3D/index-preroll-test.php&c="+Date.now();
	//google DFP
	adurl = "http://pubads.g.doubleclick.net/gampad/adx?iu=/23171097/Radio_"+call_letters+"_Web&sz=640x480&m=application/x-javascript&c="+Date.now();
	adurl = adurl.replace("REPLACEWITHPAGEALIAS",alias);
	//console.log(adurl);
	//adurl = 'http://adserver.adtechus.com/?adrawdata|3.0|5233.1|1203131|0|-1|ADTECH;alias=cfnyfm2_HOME_1x1;size=1x1;key=socastjson;loc=100;noperf=1;target=_blank;grp=13333;misc=2025176369&callback=jsonCallback&_=1373999198131';
	//adurl = 'http://adserver.adtechus.com/?adrawdata|3.0|5233.1|1203131|0|-1|ADTECH;alias=cfnyfm2_HOME_1x1;size=1x1;key=testplayer;loc=100;noperf=1;target=_blank;grp=13333;misc=1815646458&callback=jsonCallback&_=1379434631824'; //ad wrapper example
	$.ajax({
		url: adurl,
		jsonpCallback: "jsonCallback",
		dataType: "jsonp",
		timeout: 3000,
		success: function(data) {
			//test ads
			/*adPreRoll = {"audioAdImage":"",
			"clickThrough": "http://adserver.adtechus.com/adlink|5233|1191697|0|16|AdId=4112575;BnId=4;itime=933675154;key=socastjson;nodecode=yes;link=http://www.welker.ca/",
			"creative": "http://aka-cdn-ns.adtechus.com/apps/191/Ad4112575St10Sz16Sq22281548V1Id4/1x1_WELKER_COMMERCIAL.flv",
			"duration":15,
			"type":"video"};*/

			/*adPreRoll = {"audioAdImage":"http://aka-cdn-ns.adtechus.com/apps/402/Ad4232082St10Sz16Sq22397767V0Id1/55403-CKBT-Shangri-LaListenLive-300x250.jpg",
			"clickThrough":"http://adserver.adtechus.com/adlink|5233|1191697|0|16|AdId=4232082;BnId=1;itime=938756335;key=socastjson;nodecode=yes;link=http://shangrilahairandspa.ca/",
			"creative":"http://aka-cdn-ns.adtechus.com/apps/402/Ad4232082St10Sz16Sq22397767V0Id1/1x1_Shangri-La_2.mp3",
			"duration":15,
			"type":"audio"};*/
			
			//console.log('ad_data',data);
			adPreRoll = getVideoAd(data);
			
			parent.companionAd = getCompanionAdBar(data);
			parent.companionAdWrapper = getCompanionAdWrapper(data);
			//console.log(adPreRoll);
			
			$('#modal_player_overlay_background').show();
			//$('#modal_player_overlay').html('');
			
			$("#radio-player").hide();
			
			$('#listenLiveBtnBlocker').show();
			
			$("#now-playing-current").hide();
			$("#second-left").html('a few');
			
			initPrerollPlayer();
			/*if (alreadyLoaded) initPrerollPlayer(alreadyLoaded);
			else setTimeout(initPrerollPlayer, 3000);//initPrerollPlayer();*/
			
		},
		error: function(x, s, e) {
			//if (!isIE()) console.log("Error loading video ad JSON data", e);
			$('#player_wrapper_click').hide();
			closeModal(true);
		}
	});
	setCookie('socast_player_preroll',1,'session');
}

$(document).ready(function(){

	// if there is no preroll
		hasFlash = (typeof swfobject !== 'undefined' && swfobject.getFlashPlayerVersion().major !== 0);
	var hasSeenPreroll = getCookie('socast_player_preroll');
	if (hasSeenPreroll == false || hasSeenPreroll ==0) hasSeenPreroll = false;
	else hasSeenPreroll = true;
	var doNotShowPreroll = !hasFlash || hasSeenPreroll;// || isIE8;
	if (window.top !== window.self && !doNotShowPreroll) initPreroll();
	else if (window.top !== window.self && doNotShowPreroll) closeModal();

	$("#preroll-audio-link").click(function(e){
		e.preventDefault();
		window.open($(this).attr('href'));
	});
	/*$('#jp_container_preroll').live('click', function() {
		if (adPreRoll != undefined && adPreRoll && adPreRoll.clickThrough != undefined && adPreRoll.clickThrough) {
			window.open(adPreRoll.clickThrough);
		}
	});
	$(document).mousedown(function(e) {
		// The latest element clicked
		clicky = $(e.target);
		console.log(e);
	});*/
});
</script>

