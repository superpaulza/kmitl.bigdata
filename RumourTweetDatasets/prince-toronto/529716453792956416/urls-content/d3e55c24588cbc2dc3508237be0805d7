<script type="text/javascript">
$(document).ready(function(){
	var firstTimeInIframe = getCookie('first_time_in_iframe');
	setCookie('first_time_in_iframe','unset','unset');
	var songRequest ="0"; 
	try {
		if (window.top !== window.self){
			window.self.focus();// get focus for pressing CTRL+ALT+P to play or puuse radio
			// deal with iframe
			// set colume first
			if (songRequest == 1 && firstTimeInIframe != 1){
				$("#expanded-player").hide();
				$("#youtube-player").show();
								$("#listen-live").show();
								// play you tube
			}else{
				
								if (firstTimeInIframe != 1){
					$("#listen-live").show();
					var status =  getPlayerStatus();
					switch(status){
					case "play":
						startPlay();
						break;
					case "stop":
						stopPlay();
						break;
					case "pause":
						pausePlay();
						break;
					}
				}
							}
		}else{
			$("#listen-live").show();
			// deal with you tube only
			if (songRequest == 1){
				$("#expanded-player").hide();
				$("#youtube-player").show();
				// play you tube
					}
			$("#listen-live").show();
				}
	} catch(e) {
		//if (this.console) console.log('error init player in sidebar-player',e);
	}
	
	try {
		$('#radio-player-social,#youtube-player-social').hover (function(){
			//$('#sidebar-player').css('overflow','visible');
		},function() {
			//$('#sidebar-player').css('overflow','hidden');
		});
	} catch (e) {
		//if (this.console) console.log('error setting overflow for player facebook hover',e);
	}
	$(document).keydown(function(e){
		var pKeyCode = 80;
		if (e.altKey && e.ctrlKey && e.which == pKeyCode){
			e.preventDefault();
			$("#listen-live").trigger('click');
		}
	});
	
});
</script>
