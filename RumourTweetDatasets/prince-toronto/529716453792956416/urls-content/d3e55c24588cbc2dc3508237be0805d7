<script type="text/javascript">
function closeModal(forcePlay) {
	if (forcePlay === 'undefined') forcePlay = false;
	try{
		clearInterval(counter);
	}catch(err){}
	showCompanionAdBar();
	showCompanionAdWrapper();
	$('#modal_overlay').hide();
	$('#modal_player_overlay_background').hide();
	//$('#modal_player_overlay').html('');
	if (window.top !== window.self) {
		$('#now-playing-current').hide();
		$("#expanded-player").show();
		$("#radio-player").show();
	}
	
	$('#listenLiveBtnBlocker').remove();
	$("#jquery_jplayer_preroll_image").html('');
	
	// force play of stream
	//alert(getCookie('socast_player_state'));
	var cm_player_state = getCookie('socast_player_state');
	//console.log(isMobileUserAgent,hasFlash,cm_player_state,forcePlay, (window.top !== window.self));
	if (isMobileUserAgent && (cm_player_state == false || cm_player_state == 'unset')) {
		//don't autoplay on mobile
		//console.log('mobile');
	} else if (window.top !== window.self && (forcePlay || cm_player_state != 'stop')) {
		//console.log('not mobile');
		startPlay();
	}
	$("#listen-live").show();
}
$(document).ready(function () {
	if (top !== self){
	}
});

</script>

