	<script type="text/javascript">
		$(document).ready(function(){
			$("#s").focus(function(){
				if ($("#s").val().trim() == "" || $("#s").val().trim() == "Search Q107 Toronto") {
					$("#s").css("background-image","none");
					$("#s").val("");
					$("#s").css("opacity","1.0");
				}

			});
			$("#s").blur(function(){
				if ($("#s").val().trim() == "")  {
					$("#s").css("background","#E7E9E4");
					$("#s").val("Search Q107 Toronto");
					$("#s").css("opacity","0.5");
				}

			});
			$("#searchform").submit(function(e){
				if ($("#s").val().trim() == "" || $("#s").val().trim() == "Search Q107 Toronto") e.preventDefault();
			});
		});
	</script>			</div>
