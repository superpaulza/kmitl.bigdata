	<script>
	var PLAYER_ASSETS = 'http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/modules/player/images/';
	var playerType = 'songinfo';
	var streamObj = {
		m4a: "http://live.leanstream.co/CILQFM?socast_mobile",
		mp3: "http://live.leanstream.co/CILQFM-MP3?socast_mobile"
	}
	var streamURL_flv = "http://live.leanstream.co/CILQFM?type=.flv&playertype=socast1";
	var streamURL_m4a = "http://live.leanstream.co/CILQFM?socast_mobile";
	var streamURL_mp3 = "http://live.leanstream.co/CILQFM-MP3?socast_mobile";

	playClicked = function(){
				var hasHistory = !isIE8 && window.History.enabled;
		if (window.top === window.self && $("iframe#socast").length == 0 && window.History.enabled){
			
			//if (isMobileUserAgent) {
				setCookie('first_time_in_iframe',1,'session');
			//}
			
			// clear interval call first to avoid undefind called 
			var interval_id = window.setInterval("", 9999); // Get a reference to the last interval +1 
			for (var i = 1; i <= interval_id; i++)
				window.clearInterval(i);
			// clear featurerotator interval calls
			if (typeof featureRotatorIntervals != 'undefined'){
				$.each(featureRotatorIntervals,function(key,value){window.clearInterval(featureRotatorIntervals[key]);});
			}
			// clear timeout calls
			var id = window.setTimeout(function() {}, 0);
			while (id--) {
				window.clearTimeout(id); // will do nothing if no timeout with id is present
			}

			if (isIE8) {
				startPlay();
				pausePlay();
			}
			var url = window.location.href;
			//window.location = '/site/iframe?url='+encodeURIComponent(url);
			$.ajax({
				type: 'GET',
				dataType: 'html',
				url: '/site/iframe',
				data: {'url':encodeURIComponent(url), 'partial':1},
				success: function(data){
					$("#iframe-replaceholder").siblings().not("div[id^='socast_j_player_1']").not("div[id^='mobile-banner']").not("div[id^='mobile-banner-placeholder']").remove();
					//$("#iframe-replaceholder").siblings().not("div[id^='socast_j_player_1']").remove();
					$("#iframe-replaceholder").html(data);
					$(".modal").remove();
				},
				error: function(){
					//console.log("Get partial iframe view error!");
				}
			});
			if (isMobileUserAgent) {
				startPlay();//doplay();
			}
			//run tracking code
			sendStreamStarts();
			return;
		}else if(window.top === window.self && $("iframe#socast").length > 0){// mobile scenario
			if (getPlayerStatus() != 'play'){
				$("#listen-live img").attr('src',CDN_URL_CUSTOM_IMAGES+'/mobile/loader.png');
				setTimeout(check_buffering,1600);
			}else{
				$("#listen-live img").attr('src',CDN_URL_CUSTOM_IMAGES+'/mobile/SoCast_CMS-Mobile_NavIcon-Play_18x18_2X.png');
			}
			var iframe = document.getElementById("socast");
			
			// See if the lightbox is opened and close it before triggerListenLive...
			var iframe_body = window.socast.document.getElementsByTagName("body")[0];
			var lightbox_modal_overlay = $(iframe_body).find('.lightbox_modal_overlay');
			if (lightbox_modal_overlay.css('display') == 'block') {
				lightbox_modal_overlay.css('display', 'none');
			}
			var lightbox = $(iframe_body).find('.lightbox');
			if (lightbox.css('display') == 'block') {
				lightbox.css('display', 'none');
				lightbox.find('iframe').attr('src', '');
			}
			
			iframe.contentWindow.triggerListenLive();
			return;
		}else{
			return true;
		}
			};
	</script>
