<script type='text/javascript'>
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        (function() {
  var useSSL = 'https:' == document.location.protocol;
  var src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
  document.write('<scr' + 'ipt src="' + src + '"></scr' + 'ipt>');
  })();
        var type = 'Web';
        if (isMobileUserAgent) type = 'Mobile';
        var targeting = adMacros.categories;
        if (targeting == null) targeting = [];
        
        if (adMacros.postID != null) targeting.push('postID-'+adMacros.postID);
        var i = 0;
        if (adMacros.tags != null) {
                for (i=0; i< adMacros.tags.length;i++) {
                        targeting.push(adMacros.tags[i]);
                }
        }
        
        if (adMacros.eventCalendars != null) {
                for (i=0; i< adMacros.eventCalendars.length;i++) {
                        targeting.push('calendar-'+adMacros.eventCalendars[i]);
                }
        }
        if (adMacros.eventID != null) targeting.push('eventID-'+adMacros.eventID);
        
        googletag.cmd.push(function() {
                if (adMacros.type == 'adFrame' && adMacros.adSlot != undefined) {
                        if (adMacros.adSlot == 'bigbox_content')  {
                                //300x250, album/photo galleries, content grid
                                googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[300,250]],'ad-300x250-right').setTargeting("POS","right").addService(googletag.pubads());
                        }
                        if (adMacros.adSlot == 'lightbox_banner')  {
                                //300x50, gallery popup
                                googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[300,50]],'ad-300x50').addService(googletag.pubads());
                        }
                } else if (isMobileUserAgent) {
                        //320x50, mobile
                        googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[320,50]],'ad-320x50').addService(googletag.pubads());
                } else {
                        googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[300,250]],'ad-300x250-left').setTargeting("POS","left").addService(googletag.pubads());
                        googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[728,90]],'ad-728x90').addService(googletag.pubads());
                        if (adMacros.type == 'post' || adMacros.type == 'event' || adMacros.type == 'polls' || adMacros.type == 'page') {
                                //640x125, blog header
                                googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[640,125]],'ad-640x125').addService(googletag.pubads());
             
                        }
                        if (call_letters == "CHEXTV" || call_letters == "CKWSTV" || call_letters == "CHANTV"){
                            //right column 300x50
                            googletag.defineSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, [[300,50]],'ad-300x50').addService(googletag.pubads());
                        }
                        googletag.defineOutOfPageSlot('/23171097/RADIO_'+call_letters+'_'+type+'/'+alias, 'div-oop').addService(googletag.pubads());           
                }

                //Base pass url keyword for all ads on the page
                googletag.pubads().setTargeting("url",adMacros.url.pathname).setTargeting("sec",targeting); 
                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableSyncRendering();
                googletag.pubads().collapseEmptyDivs();

                googletag.enableServices();
        });
</script>
<script>
var adServer = 'google';
function adRender(a, c, s, postfix) {
    if (s == '728x90') {
            document.write("<div id='ad-728x90' style='display:inline-block;'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-728x90'); });</scr"+"ipt></div>");
    }
    if (s == '300x250' && postfix.toLowerCase() == 'left') {
            document.write("<div id='ad-300x250-left'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-300x250-left'); });</scr"+"ipt></div>");
    }
    if (s == '300x250' && postfix == 'CONT') {
            document.write("<div id='ad-300x250-right' ><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-300x250-right'); });</scr"+"ipt></div>");
    }
    if (s == '320x50') {
            document.write("<div id='ad-320x50' style='display:inline-block;'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-320x50'); });</scr"+"ipt></div>");
    }
    if (s == '300x50') {
        if(postfix == "_001"){
            document.write("<a href='http://pubads.g.doubleclick.net/gampad/jump?iu=/23171097/RADIO_"+call_letters+"_"+type+"/"+alias+"&sz=300x50&t=POS%3D1%26url%3D"+adMacros.url.pathname+"&c="+Math.round(new Date().getTime() / 1000)+"' target='_blank'><img src='http://pubads.g.doubleclick.net/gampad/ad?iu=/23171097/RADIO_"+call_letters+"_"+type+"/"+alias+"&sz=300x50&t=POS%3D1%26url%3D"+adMacros.url.pathname+"&c="+Math.round(new Date().getTime() / 1000)+"'/></a>");
        } else if (postfix == "_002"){
            document.write("<a href='http://pubads.g.doubleclick.net/gampad/jump?iu=/23171097/RADIO_"+call_letters+"_"+type+"/"+alias+"&sz=300x50&t=POS%3D2%26url%3D"+adMacros.url.pathname+"&c="+Math.round(new Date().getTime() / 1000)+"' target='_blank'><img src='http://pubads.g.doubleclick.net/gampad/ad?iu=/23171097/RADIO_"+call_letters+"_"+type+"/"+alias+"&sz=300x50&t=POS%3D2%26url%3D"+adMacros.url.pathname+"&c="+Math.round(new Date().getTime() / 1000)+"'/></a>");
        } else{
            document.write("<div id='ad-300x50;'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-300x50'); });</scr"+"ipt></div>");
        }
            
    }
    
    if (s == '640x125') {
            document.write("<div id='ad-640x125'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('ad-640x125'); });</scr"+"ipt></div>");
    }
    if (s == 'oop') {
            document.write("<div id='div-oop'><scr"+"ipt type='text/javascript'>googletag.cmd.push(function() { googletag.display('div-oop'); });</scr"+"ipt></div>");
    }
}
