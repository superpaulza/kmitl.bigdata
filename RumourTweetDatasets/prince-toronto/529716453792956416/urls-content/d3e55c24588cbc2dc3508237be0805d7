		$("#desktop_forgot_modal-progress").hide();
	});
</script>						</div>
					</div>
				</div>
			</div>
						
		</div>
		</form>	</div>
</div>

<script type="text/javascript">
$(document).ready(function(){	
	$("#desktop_forgot_form").submit(function() {
		$("#desktop_forgot_modal-progress").show();
		var username = $("#desktop_forgot_form input[name='LoginForm[username]']").val();
		$.ajax({
			type: "POST",
			datatype: "html",
			url: "/login/forgotPassword",
			data: { "LoginForm[username]" : encodeURIComponent(username)
					 },
			success: function(data){
				//console.log(data);
				$("div.errorMessage-desktop_forgot_modal").html(data);
			},
			error: function(){
				//console.log("oops");
			},
			complete: function(){
				$("#desktop_forgot_modal-progress").hide();
			}
		});
		return false;
	});
});
</script>


<script type="text/javascript">
$(document).ready(function(){
	
});
</script>
<script type="text/javascript">
$(document).ready(function(){

	// setup login modal
	$("#log-in, #respond .must-log-in a,#comments .commentlist .comment .reply a, .openLogin").live('click',function(){
		//window.parent.openLogin();
		openLogin();
	});
		
    $("#desktop_login_form .forgot-password").click(function(){
		openForgotPassword();
		return false;
	});
	
	// setup login modals
	function openLogin() {
		$('#desktop_forgot_modal').dialog('close');
		$('#desktop_login_modal').dialog('open');
	}
	
	function openForgotPassword() {
		$('#desktop_login_modal').dialog('close')
		$('#desktop_forgot_modal').dialog('open');
	}
	
	var widthOfWindow = $(window).width();
	var widthOfModal = widthOfWindow - 40;
	if (widthOfModal > 450) widthOfModal = 450;
	$('#desktop_login_modal').dialog({
		title: 'Login',
		autoOpen: false,
		modal: true,
		resizable:false,
		position:{my:"center",at:"center center",of:"#navigation-area"},
		width:widthOfModal,
		create: function() {
		},
		open: function(){
			if ($.browser.msie) $("#flash").css('visibility','hidden');
			//$("#desktop_forgot_modal").dialog('close');
		    $(this).keyup(function(e) {
		        if (e.keyCode == 13) {
		        	$("#desktop_login_form").submit();
		        }
		    });
		},
		close: function(){
			$("#flash").css('visibility','visible');
			$('#desktop_login_form .errorMessage-desktop_login_modal').html('');
			$('#LoginForm_username').val('');
			$('#LoginForm_password').val('');
		},
		buttons: {
			'Login': function() {
				$("#desktop_login_form").submit();
			},
			'Sign Up': function() {
				$('#desktop_login_modal').dialog('close');
				//window.open('/community/signup', 'contentFrame', '');
				window.location = "/signup";
			}
		}
	});
	
	$('#desktop_forgot_modal').dialog({
		title: 'Forgot Password',
		autoOpen: false,
		modal: true,
		resizable:false,
		position:{my:"center",at:"center center",of:"#navigation-area"},
		width:widthOfModal,
		create: function() {
		},
		open: function(event, ui){
			if ($.browser.msie) $("#flash").css('visibility','hidden');
			$(this).keyup(function(e) {
		        if (e.keyCode == 13) {
			    	$("#desktop_forgot_form").submit();
				}
		    });
		},
		close: function(){
			$("#flash").css('visibility','visible');
		},
		buttons: {
			'Retrieve Password': function() {
				$("#desktop_forgot_form").submit();
			},
			'Login': function() {
				openLogin();
			},
			'Sign Up': function() {
				$('#desktop_forgot_modal').dialog('close');
				//window.open('/community/signup', 'contentFrame', '');
				window.location = '/signup';
			}
		}
	});
});	
</script>		
				
		</div><!-- iframe-replaceholder -->
		
