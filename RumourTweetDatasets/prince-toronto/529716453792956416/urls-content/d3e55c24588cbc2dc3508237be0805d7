<script type="text/javascript">
// defined these previousOnes to get rid of the incorrect multiple updating within a small time frame
var previousArtistName1 = '';
var previousSongName1 = '';
var previousArtistName2 = '';
var previousSongName2 = '';
var previousArtistName3 = '';
var previousSongName3 = '';

var artistPictures = null;
var artistPictureIndex = 0;
var currentTime = new Date().getTime() / 1000;
var globalCounter = 0;

var playerType = 'songinfo';
var animation_is_on = (playerType == 'songinfo');
var animation_type = 'fade';//fade or slide

$(document).ready(function(){
	if (playerType == 'songinfo') {
		$(window).bind('orientationchange', function(){
			resizeCanvas();
		}).trigger('orientationchange');

		$(window).resize(function(){
			resizeCanvas();
		});

		redrawPicture();
	} else {
		showProgramInfo();
		if (window.top === window.self) {
			$('#modal_overlay').hide();
			$('#modal_player_overlay_background').hide();
			$('#now-playing-current').hide();
			$('#player-control').hide();
			$("#expanded-player").show();
			$("#radio-player").show();
		}
	}
});


///////////////////////////////////////////////////////////////////////
// Slide Show
///////////////////////////////////////////////////////////////////////
function getCanvasWidthHeight(image){
	var windowWidth = 303;//parseInt($(window).width());
	var windowHeight = 190;//parseInt($(window).height());
	var height = windowHeight; //parseInt((windowHeight-60) * 0.8);
	var width = windowWidth; //parseInt(windowWidth * 0.92);
	var naturalWidth = image.naturalWidth? image.naturalWidth : image.width;
	var naturalHeight = image.naturalHeight? image.naturalHeight : image.height;
	var imageWidth = naturalWidth;
	var imageHeight = naturalHeight;

	// fit image into container
	if ((width/height) > (imageWidth/imageHeight)) {
		animation = "down";
		imageWidth = width;
		imageHeight = parseInt(naturalHeight * (imageWidth/naturalWidth));
	} else {
		animation = "left";
		imageHeight = height;
		imageWidth = parseInt(naturalWidth * (imageHeight/naturalHeight));
	}
	
	return {
		"width":width,
		"height":height,
		"animation":animation,
		"imageWidth":imageWidth,
		"imageHeight":imageHeight
		};
}

function resizeCanvas() {
	return;

	// only change the container size and image size
	var canvas = getCanvasWidthHeight($("#canvas-image img"));
	$("div.jp-audio-socast div.jp-type-single div.jp-interface")
		.width('100%')
		.height(canvas["height"]);

	// change canvas size
	$("#canvas-image img")
		.stop()
		.css({
			'min-width':canvas["imageWidth"]+'px',
			'min-height':canvas["imageHeight"]+'px'
		});
}

var redrawTimer = null;
function redrawPicture(){
	if (animation_is_on && animation_type == 'fade'){
		//$("#canvas-picture").empty().show();
		$("#canvas-picture,#canvas-image,#canvas-image-old").css({
			'margin-left':'auto',
			'margin-right':'auto',
			'width':'300px',
			'height':'190px'
		});
		$("#canvas-picture").show();
		var images = $("#artist-gallery").children();
		if (images.length <= 0) images = $("#station-gallery").children(); // use station ad
		if (artistPictureIndex >= images.length) artistPictureIndex = 0;
		
		if ($("#canvas-image").length < 1) $("#canvas-picture").append('<div id="canvas-image"></div>');
		if ($("#canvas-image-old").length < 1) $("#canvas-picture").append('<div id="canvas-image-old"></div>');
		$("#canvas-image-old").html($("#canvas-image").html());
		$("#canvas-image-old img").ready(function() {
			$("#canvas-image").html('').hide();
			$(images[artistPictureIndex]).clone().appendTo('#canvas-image');
			setCookie('socast_player_artist_image',"<img id='" + $(images[artistPictureIndex]).attr('id') +"' src='" + $(images[artistPictureIndex]).attr('src') + "' alt='artist' />",'session');
			$("#canvas-image img").ready(function() {
				var canvas = getCanvasWidthHeight($("#canvas-image img")[0]);
				$("div.jp-audio-socast div.jp-type-single div.jp-interface")
					.width('100%')
					.height(canvas["height"]);
				$("#canvas-image img")
					.stop()
					.css({
						'min-width':canvas["imageWidth"]+'px',
						'min-height':canvas["imageHeight"]+'px'
					});
				$("#canvas-picture img[id^='station-']").css({
						'min-width':'0px',
						'min-height':'0px',
						'max-width':'300px',
						'max-height':'190px',
						'top':'10px'
					});

				if (redrawTimer) clearTimeout(redrawTimer);
				if (canvas['animation'] == 'left' && canvas["imageWidth"]-canvas["width"]>10) {
					$("#canvas-image img").css("left", "0px");
				} else if (canvas['animation'] == 'down' && canvas["imageHeight"]-canvas["height"]>10) {
					$("#canvas-image img").css("top", "0px");
				}
				$("#canvas-image").fadeIn(1000,function() {
					if (images.length > 1) redrawTimer = setTimeout(redrawPicture, 15000);
					artistPictureIndex++;
				});
			});
		});
		
	} else { 
		$("#canvas-picture").fadeOut('normal',function(){
			$("#canvas-picture").empty().show();
			var images = $("#artist-gallery").children();
			if (images.length <= 0)
				images = $("#station-gallery").children(); // use station ad
			if (artistPictureIndex >= images.length) 
				artistPictureIndex = 0;
			
			$("#canvas-picture").append('<div id="canvas-image"></div>');
			$(images[artistPictureIndex]).clone().appendTo('#canvas-image');
			//$("#canvas-image img").hide();

			$("#canvas-image img").ready(function() {
				var canvas = getCanvasWidthHeight($("#canvas-image img")[0]);
				$("div.jp-audio-socast div.jp-type-single div.jp-interface")
					.width('100%')
					.height(canvas["height"]);
				$("#canvas-image img")
					.stop()
					.css({
						'min-width':canvas["imageWidth"]+'px',
						'min-height':canvas["imageHeight"]+'px'
					});
				$("img#station-image").css({ // this top will be overwritten by line#46
						'min-width':'0px',
						'min-height':'0px',
						'max-width':'300px',
						'max-height':'190px',
						'top':'10px'
					});

				if (redrawTimer) clearTimeout(redrawTimer);
				if (canvas['animation'] == 'left' && canvas["imageWidth"]-canvas["width"]>10) {
					// slide left
					if (animation_is_on && animation_type == 'slide'){
						$("#canvas-image img")
							.css("left", canvas["width"]-canvas["imageWidth"])
							.animate({left: '0px'}, 15000, function() { 
								if (images.length > 1)
									redrawTimer = setTimeout(redrawPicture, 15000);
							}
						);
					} else {
						$("#canvas-image img").css("left", "0px");
					}
				} else if (canvas['animation'] == 'down' && canvas["imageHeight"]-canvas["height"]>10) {
					// slide down
					if (animation_is_on && animation_type == 'slide'){
						$("#canvas-image img")
							.css("top", canvas["height"]-canvas["imageHeight"])
							.animate({top: '0px'}, 15000, function() { 
								if (images.length > 1)
									redrawTimer = setTimeout(redrawPicture, 15000);
							}
						);
					} else {
						$("#canvas-image img").css("top", "0px");
					}
				} else {
					if (images.length > 1)
						redrawTimer = setTimeout(redrawPicture, 15000);
				}
			});
			artistPictureIndex++;
		});
	}
}

constructSongLink = function(stationID,artistName,songName){
	songLink = "http://www.q107.com/player/player/?stationID="+stationID+"&artistName="+artistName+"&songName="+songName;
	return songLink;
}

function startUpdateSong(initial) {
	try  {
		if (!parent.metaIsWorking) {
			if (initial) updateSong();
			else {
				clearTimeout(parent.updateSongTimeout);
				parent.updateSongTimeout = setTimeout(updateSong, 60000);
			}
		}
	} catch(e) {
	}
}
function stopUpdateSong() {
	try  {
		if (!parent.metaIsWorking) {
			clearTimeout(parent.updateSongTimeout);
		}
	} catch(e) {
	}
}


function updateSong() {
	if (!parent.metaIsWorking && !playerIsVisible()) {
		startUpdateSong();
		return;
	}
	parent.songIsChanged = false;
	if (playerType == 'program') showProgramInfo();
	else {
		try {
			listenLogData = {
				"cmd":"getLastPlayedSongs",
				"stationID": "5",
				"mode": "radio",
				"playerType": "website",
				"includeNowPlaying": 1,
				"lastPlayedLimit": 6,
				"cacheKill": Math.random()
			};
			
			if (typeof moreClicks !== 'undefined' && typeof numSongsPerPage !== 'undefined') {
				listenLogData.lastPlayedLimit = (moreClicks*numSongsPerPage)+1;
			}
			
			if (parent.metaIsWorking) {
				listenLogData.artistName = meta_artist;
				listenLogData.songName = meta_song;
			}
					  
			$.ajax({
				type: 'GET',
				dataType: 'json',
				url: '/player/servicePlayer',
				data: listenLogData,
				timeout: LISTEN_LOG_TIMEOUT,
				success: function(data) {
					if (data.lastPlayedSongs !== undefined) {
						var lastPlayedSongs = data.lastPlayedSongs;
						var last_played = '<div class="last-played-item first"><h2>Last Played</h2></div>';
						if (lastPlayedSongs.lastPlayedTemplate !== undefined && lastPlayedSongs.songs !== undefined && lastPlayedSongs.songs.length > 0) {
							for (var i=0; i< lastPlayedSongs.songs.length; i++) {
								last_played += lastPlayedSongs.lastPlayedTemplate.replace(/{SONG_URL}/g,lastPlayedSongs.songs[i].song_url).replace(/{THUMB_IMG}/g,lastPlayedSongs.songs[i].thumb_img).replace(/{SHORT_ARTIST_NAME}/g,lastPlayedSongs.songs[i].short_artist_name).replace(/{SHORT_SONG_NAME}/g,lastPlayedSongs.songs[i].short_song_name).replace(/{LAST_TIME_WORDS}/g,lastPlayedSongs.songs[i].last_played_words);
							}
						} else {
							last_played += '<div class="last-played-item">No songs found.</div>';
						}
						
						$('#last-played').html(last_played);
						$('#last-played .last-played-item').last().addClass('last');
					}
					
					if (data.nowPlaying !== undefined) {
						radioSetNowPlaying(data.nowPlaying);
					}
				},
				complete: function(data) {
					startUpdateSong();
				}
			});
		} catch(e) {
			//if(window.console) console.log('UpdateSong Error: ',e);
		}
	}
}

function radioSetNowPlaying(song) {
	if (song != false){
		if (playerType == 'songinfo') showSongInfo(song);
		else if (playerType == 'program') showProgramInfo();
	} else if (playerType == 'songinfo') {
		$("#artist-gallery").html('');
		artistPictures = null;
		redrawPicture();
		$("#radio-player-artist-name").html("Q107 Toronto");
		$("#radio-player-song-name").html("");

		// update facebook url
		$('.fb-like').attr('data-href','http://www.q107.com');
		$('.fb-like').html('<fb:like href="http://www.q107.com" send="false" width="100" show_faces="true" layout="button_count"></fb:like>');
		FB.XFBML.parse();
	}
}


// Listen logger
///////////////////////////////////////////////////////////////////////////
var LISTEN_LOG_INTERVAL = 6000; // ms between each interval
var LISTEN_LOG_TIMEOUT = 3000; // timeout ms
var LISTEN_LOG_ERROR = 5; // num errors until abort
//var listenLogTimer = null; moved to  yii layouts/playerIframe.php
var listenLogData = null;
var listenLogErrors = 0;
var listenLogLastTime = new Date().getTime();
var listenLogLastResponse = null;
var listenLog_diff = 0;
function updateListenLog() {
	return;
	try{
	
		//clearInterval(listenLogTimer);
	
		// determine if interval is correct
		var currTime = new Date().getTime();
		listenLog_diff = currTime-listenLogLastTime;
		if (listenLogLastTime > 0) {
			//if (!isIE()) console.log("updateListenLog: last update was ", listenLog_diff, 'miliseconds ago');
		}
		listenLogLastTime = currTime;

		// check if number of errors exceeded
		if (listenLogErrors >= LISTEN_LOG_ERROR) {
			  //if (!isIE()) console.log("startListenLog: more than ", LISTEN_LOG_ERROR, " errors, aborting");
			  stopListenLog();
		} else {
			  // do update
			  listenLogData = {
					"cmd":"updateListenLog",
					"stationID": "5",
					"mode": "radio",
					"playerType": "website",
					"includeNowPlaying": 1,
					"listenLogTime":listenLog_diff,
					"cacheKill": Math.random()
			  };
						  
			  $.ajax({
					type: 'GET',
					dataType: 'json',
					url: '/player/servicePlayer',
					data: listenLogData,
					timeout: LISTEN_LOG_TIMEOUT,
					success: function(response) {
						if (response.nowPlaying != false){
							var songData = response.nowPlaying;
							previousArtistName1 = previousArtistName2;
							previousSongName1 = previousSongName2;
							previousArtistName2 = previousArtistName3;
							previousSongName2 = previousSongName3;
							previousArtistName3 = songData['artist_name'];
							previousSongName3 = songData['song_name'];
							if (!isIE()){
								//console.log("previousSongName1="+previousSongName1);
								//console.log("previousSongName2="+previousSongName2);
								//console.log("previousSongName3="+previousSongName3);
							}
							if (previousArtistName3 == previousArtistName1 && previousSongName3 == previousSongName1 && previousSongName3 != "undefined"){
								previousSongName2 = previousSongName3;
								previousArtistName2 = previousArtistName3;
							}
							if (previousArtistName3 == previousArtistName2 
									&& previousSongName3 == previousSongName2
									&& (previousArtistName3 != previousArtistName1 
										|| previousSongName3 != previousSongName1)
							){
								if (!metaIsWorking && playerType == 'songinfo') showSongInfo(response.nowPlaying);
								if (playerType == 'program') showProgramInfo();
							}else{
								//if (!isIE()) console.log("updateListenLog: same song, do nothing");
							}
							// keep update your variables
							listenLogLastResponse = response.nowPlaying;
							listenLogErrors = 0;
						} else if (playerType == 'songinfo') {
							previousArtistName1 = previousArtistName2;
							previousSongName1 = previousSongName2;
							previousArtistName2 = previousArtistName3;
							previousSongName2 = previousSongName3;
							previousArtistName3 = "Q107 Toronto";
							previousSongName3 = '';
							if (previousArtistName3 == previousArtistName1 && previousSongName3 == previousSongName1 && previousSongName3 != "undefined"){
								previousSongName2 = previousSongName3;
								previousArtistName2 = previousArtistName3;
							}
							if (previousArtistName3 == previousArtistName2 
									&& previousSongName3 == previousSongName2
									&& (previousArtistName3 != previousArtistName1 
										|| previousSongName3 != previousSongName1)
							) {
								$("#artist-gallery").html('');
								artistPictures = null;
								redrawPicture();
								$("#radio-player-artist-name").html("Q107 Toronto");
								$("#radio-player-song-name").html("");
					
								// update facebook url
								$('.fb-like').attr('data-href','http://www.q107.com');
								$('.fb-like').html('<fb:like href="http://www.q107.com" send="false" width="100" show_faces="true" layout="button_count"></fb:like>');
								FB.XFBML.parse();
							}
						}
					},
					error: function() {
						  //if (!isIE()) console.log("updateListenLog: ajax error");
						  listenLogErrors++;
					},
					complete: function(){
						//startListenLog();
					}
			  });   
		}
	} catch(e) {
		//if (!isIE()) console.log('Error: updateListenLog',e);
	}
}
function startListenLog() {
	try{
		startUpdateSong();
		if (parent.listenLogTimer) {
			clearInterval(parent.listenLogTimer);
			parent.listenLogTimer = null;
		}
		parent.listenLogTimer = setInterval(updateListenLog, LISTEN_LOG_INTERVAL);
	} catch(e) {
		//if (!isIE()) console.log('Error: startListenLog',e);
	}
}
function stopListenLog() {
	try {
		stopUpdateSong();
		if (parent.listenLogTimer) {
			  clearInterval(parent.listenLogTimer);
			  parent.listenLogTimer = null;
		}
	} catch(e) {
		//if (!isIE()) console.log('Error: stopListenLog',e);
	}
}
function showSongInfo(song){
	try {
		/* the "song" is ignored due to no artist picture brought in
		var ajax_data = {"stationID": "5"};
		if (metaIsWorking) {
			ajax_data.artistName = meta_artist;
			ajax_data.songName = meta_song;
		}
		$.ajax({
			type: 'GET',
			dataType: 'json',
			url: '/player/getCurrentSongOnAir',
			data: ajax_data,
			success: function(songData){*/
				var songLink = constructSongLink(5,song.artist_name,song.song_name);
				var artistHtml = song.artist_name;
				var songHtml = song.song_name;
				var onAir = '<ul class="on-air"><li>ON AIR NOW</li></ul>';
				// update info for radio
				$("#radio-player-artist-name").html(artistHtml);
				$("#radio-player-song-name").html(songHtml);
				//$("#on-air-now-radio").html(onAir);
	
				// update facebook url
				$('.fb-like').attr('data-href',songLink);
				$('.fb-like').html('<fb:like href="'+songLink+'" send="false" width="100" show_faces="true" layout="button_count"></fb:like>');
				FB.XFBML.parse();
	
				if (typeof(song) !== "undefined" && typeof(song.pictures) !== "undefined" && typeof(song.pictures[0]) !== "undefined"){
					artistPictureIndex = 0
					artistPictures = song.pictures;
					$("#artist-gallery").html('');
					if (animation_is_on){
						for(var i=0; i < song.pictures.length; i++){
							$("#artist-gallery").append("<img id='artist-image-" + i +"' src='" + song['pictures'][i] + "' alt='' />");
						}
					}else{
						var i = Math.floor(Math.random() * song.pictures.length);
						if (i == song.pictures.length) i = song.pictures.length-1;
						$("#artist-gallery").append("<img id='artist-image-" + i +"' src='" + song.pictures[i] + "' alt='' />");
						setCookie('socast_player_artist_image',"<img id='artist-image-" + i +"' src='" + song.pictures[i] + "' alt='' />",'session');
						//if(!isIE()) console.log(song['pictures']);
					}
				}else{
					//console.log('no pictures');
					$("#artist-gallery").html('');
					artistPictures = null;
				}
				redrawPicture();
				//updateLastPlayed();
			/*},
			error: function(){
				//if (!isIE()) console.log("Error: getCurrentSongOnAir");
			},
			complete:function(){
				// TODO
			}
		});*/
	} catch (e) {
		//if (!isIE()) console.log('Error: showSongInfo',e);
	}
}
	
	
var meta_artist = '';
var meta_song = '';
var meta_title = '';
function parseMeta(event) {
	if (playerType == 'program') return;
	if(window.top === window.self && $("iframe#socast").length > 0) {
		var iframe = document.getElementById("socast");
		iframe.contentWindow.parseMeta(event);
		return;
	}
	if (parent.windowIsBlurred) {
		parent.songIsChanged = true;
	}
	//if (window.console) console.log(event.metadata);
	if (event.metadata.title != undefined && event.metadata.title != '') {
		parent.metaIsWorking = true;
		meta_title = event.metadata.title.split(' - ');
		if (meta_title.length == 2) {
			meta_artist = meta_title[1];
			meta_song = meta_title[0];
			//showSongInfo({});
		} else {
			meta_artist = 'Q107 Toronto';
			meta_song = '';
		}
		//updateListenLog();
		if (!parent.windowIsBlurred) updateSong();
		setTimeout(clearBuffer,3000);
	} else {
		meta_artist = 'Q107 Toronto';
		meta_song = '';
	}
	$('#radio-player-artist-name').html(meta_artist);
	$('#radio-player-song-name').html(meta_song);
}

function showProgramInfo() {
	var programHeaderImg = 'http://d2x3wmakafwqf5.cloudfront.net/uploads/station/5/cmsSiteLogo.png?r=5368';
programHeaderImg = "http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/blogs.dir/61/files/2014/10/OnAirNow_BlackandWhite_Nails1.png";	$('#canvas-picture').empty();
	$("#canvas-picture").append('<div id="canvas-image"></div>');
	$('#canvas-image').append('<img class="program-image" src="' + programHeaderImg + '" alt="Nails Mahoney" />');
	$('#canvas-picture').append('<div class="program-schedule">Q107 Toronto</div>');

	var $p = $('div.hidden.html div.news-player-program-info');
	if ($p.length > 0) $('#canvas-picture div.program-schedule').html($p.html());
	$('#canvas-picture').append('<div class="clear"></div>');
	
	var programLink = '';
	programLink = "http://www.q107.com/nailsmahoney";
	$('#radio-player-social').show();
	$('.fb-like').attr('data-href',programLink);
	$('.fb-like').html('<fb:like href="'+programLink+'" send="false" width="100" show_faces="true" layout="button_count"></fb:like>');
	if (typeof FB != 'undefined') FB.XFBML.parse();
}

