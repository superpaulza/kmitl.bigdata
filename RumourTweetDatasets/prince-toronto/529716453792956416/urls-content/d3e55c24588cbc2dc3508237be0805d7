<script type="text/javascript">
$(document).ready(function(){
	
	$("#desktop_login_form").submit(function() {
		$("#desktop_login_modal-progress").show();
		var username = $("#desktop_login_form input[name='LoginForm[username]']").val();
		var password = $("#desktop_login_form input[name='LoginForm[password]']").val();
		var rememberMe = parseInt($("#desktop_login_form input[type='checkbox'][name='LoginForm[rememberMe]']:checked").val() ) || parseInt($("#desktop_login_form input[type='hidden'][name='LoginForm[rememberMe]']").val());
		$.ajax({
			type: "POST",
			datatype: "html",
			url: "/login",
			data: { "LoginForm[username]" : encodeURIComponent(username),
					"LoginForm[password]" : encodeURIComponent(password),
					"LoginForm[rememberMe]" : encodeURIComponent(rememberMe)
					 },
			success: function(data){
				/** bug#2612  Remove Emmis integration
				if (data == "ThirdPartyLoggedIn"){
					try{
						$('#desktop_login_modal').dialog('close');
					}catch(e){}
					$('#agree-modal').data('user',{'username': username, 'password': password }).dialog('open');
				}
				else */if (data == 'activate'){
					try{
						$('#desktop_login_modal').dialog('close');
					}catch(e){}
										window.location.href = 'http://'+window.location.host+'/community/activate';
				}
				else{
					$("div.errorMessage-desktop_login_modal").html(data);
					if (data.toString().indexOf('Sorry') == -1){

						
						if (checkAndGoReturnUrlInCookie()) return;

													var href = document.referrer;
															$('#desktop_login_modal').dialog('close');
								href = window.location.href;
															if (href.indexOf('/community/mobileLogin') >= 0 || href.indexOf('/community/member') >= 0){
									href = '/setting';
								}
								window.location = href;
						
																
					}
				}
					
			},
			complete: function(){
				$("#desktop_login_modal-progress").hide();
			}
		});

		return false;
	});
	
});
</script>

