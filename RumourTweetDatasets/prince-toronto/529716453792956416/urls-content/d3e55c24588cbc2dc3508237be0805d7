<script type="text/javascript">
	jQuery(document).ready(function(){
		var url = "www.q107.com/2014/11/04/prince-playing-a-surprise-show-in-toronto-tonight/";
		// add first(home) and last nav(li)
		var first = '<li class="first"><a href="http://www.q107.com/"><img src="http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/corusmusic/images/SoCast_CMS-Nav_HomeBtn_20x20.png"  alt="home" id="navHomeBtn" /><img src="http://d2x3wmakafwqf5.cloudfront.net/wordpress/wp-content/themes/corusmusic/images/SoCast_CMS-Nav_HomeBtnOver_20x20.png"  alt="home" id="navHomeBtnOver" /></a></li>';
		var last = '<li class="last"></li>';
		jQuery("#navigation-area-menu ul.menu").prepend(first).ready(function(){
			jQuery("#navigation-area-menu ul.menu").append(last).ready(function(){
				highlight();
			});
		});
		// make nav clickable
		$(".menu li.first a, ul [id^='menu-item-'] a").live('click',function(){
				if ($(this).attr('target') == '_blank') {
					window.open($(this).attr('href'));
				}
				else {
					document.location.href = jQuery(this).attr('href');
				}
		});
		// generate secondary nav
		showSecondaryNav = function(container,activeItem){
			var secondNavs = '';
			var firstClass = '';
			var activeClass = '';
            var hideClass = '';
			jQuery(container).children("ul").find("li").each(function(){
				firstClass = '';
				activeClass = '';
                hideClass = '';
				if (secondNavs == '') firstClass = ' first';
				if (jQuery(this).find('a').attr('href').indexOf(url) != -1) activeClass = ' active';
                if (jQuery(this).hasClass('hide')) hideClass = ' hide';
				secondNavs = secondNavs + '<li class="secondary-link' + firstClass + activeClass + hideClass + '">' + jQuery(this).html()+"</li>";
			});
			jQuery("#secondary-nav").html(secondNavs);
			jQuery("#secondary-nav").css('display','block');
		}
			
		highlight = function(){
			var menu = $("ul#menu-socast-nav");
			$(menu).children().each(function(index,child){
				var childAnchor = $(child).find("a");
				if (childAnchor != undefined){
					var childHref = $(childAnchor).attr('href');
					if (childHref != undefined){
						var childHref = childHref.split('//');
						for (var i=0; i<childHref.length;i++){
							if (childHref[i] == url){
								$(child).addClass("active");
								showSecondaryNav(child);
							}
						}
					}
				}
				var childMenu = $(child).find("ul");
				if (childMenu != undefined){
					$(childMenu).children().each(function(idx, subChild){
						var subChildAnchor = $(subChild).find("a");
						if (subChildAnchor != undefined){
							var subChildHref = $(subChildAnchor).attr('href');
							if (subChildHref != undefined){
								subChildHref = subChildHref.split('//');
								for (var i=0; i<subChildHref.length;i++){
									if (subChildHref[i] == url){
										$(child).addClass("active");
										$(subChild).addClass("hover");
										showSecondaryNav(child);
									}
								}
							}
						}
						
					});
				}
			});
		}
	});
</script>
				
