<script type="text/javascript">

  // Triggered by VideoPlayer.vm
  jQuery(document).bind('isDone.initPlayerVidNav', function() { outputLog("ETS", "About to call initPlayer()"); initPlayer(576405); });
  
  requiresDependency('http://static.ctvnews.ca/bellmedia/common/js/videoNavigation.js', function() {
    /* ----------------- Event binding ---------------------- */
    // If clipId is not null, send clip to player when player is ready.
    jQuery(document).bind('isDone.onPlayerReady', function() {
        setPFUserIP("137.205.117.66");
        setPFClientSite("ottawa.ctvnews.ca");
        setPFCanonicalUrl("%2Fcm%2F2.628%2F2.629%2F2.627%2Fvideo%3FclipId%3D483837%26playlistId%3D1.2086170%26binId%3D1.1164511%26playlistPageNum%3D1");
        
        updatePlayerAdSettings("", "");
        //setPlayerAdSite("");
        getClips("1.2086170", "1", "25", "true", "483837", '#videoContainer');
    });
    
    // Whenever a video starts, update the breadcrumb and share path.
    jQuery(document).unbind('isDone.onVideoStartForNav').bind('isDone.onVideoStartForNav', function(event, clipId, clipTitle) {
        if ( clipId != -1 ) {
          // clipId -1 means it is a commercial.
          updateSharePath(clipId, playlistIdShare, binIdShare, clipTitle, 'http://ottawa.ctvnews.ca/video', currPlayingClipPageNum);
          updatePlaylistTitleMobile(clipTitle);
          var sharePath = updateSharePath(clipId, playlistIdShare, binIdShare, clipTitle, 'http://ottawa.ctvnews.ca/video', currPlayingClipPageNum);
                      renderGigyaShares(clipTitle, sharePath, "never", "http://static.ctvnews.ca");
                  }
    });
                                            
    /* ----------------- Event binding: Playlist logic ---------------------- */   
    // This event is triggered when a user selects a clip or it moved on to the next page of clips.                                            
    jQuery(document).bind('isDone.onPlaylistUpdate', function() {
      
      setCurrPlayingClipPageNum( getPlaylistPageNum() );
    });
    
    // This event is triggered when the player has finished playing the playlist of clips one a single page of clips.
    jQuery(document).bind('isDone.onPlaylistComplete', function() {
        var nextPageNum = getCurrPlayingClipPageNum() + 1;
        
        if ( nextPageNum <= getTotalPagesOfClips() && checkClipIdInList == true) {
          getClips(binIdShare, nextPageNum, '25', 'true', null, '#videoContainer');
        }
    });
    
  });
  
  /* -- Navigation logic -- */
  jQuery(document).ready(function() {
    for ( var i = 0; i < nav_contentIdArray.length; i ++ ){
      var contentId = nav_contentIdArray[i];
      var contentIdFormated = contentId.replace(".", "\\.");
      jQuery("#"+contentIdFormated).bind( "click", {id: contentId}, function(event) {
        var data = event.data;
        //getPlaylists(data.id, '1', '25');
        getClips(data.id, '1', '25', 'true');
        setBinIdForSharing(data.id);
        return false;
      });
    }
  });
</script>

  
