    function DARTLegolasGetCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') c = c.substring(1, c.length);
          if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }
    var segments = [];
    //Read the cookie
    var lsg = DARTLegolasGetCookie('lsg');
    if (lsg) {
      //Tokenize
      var cookieTokens = this.lsg.split('s');

      //Build string for dart requests
      for(var i=0; i < cookieTokens.length; i++) {
        if (cookieTokens[i] != '0') { 
          segments.push(cookieTokens[i]); 
        }
      }
    }
//--><!]]>
