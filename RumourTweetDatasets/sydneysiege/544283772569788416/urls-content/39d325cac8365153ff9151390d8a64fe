		Livefyre.require(['fyre.conv#3', 'auth'], function(Conv, auth) {
			new Conv(networkConfig, [convConfig]);

			auth.delegate({
				login: function() {
					window.location.assign('https://skyid.sky.com/signin/news?successUrl=' + window.location.href);
				},

				logout: function(errback) {
					errback(null); // Notifies Livefyre that logout was successful

					window.location.assign('https://skyid.sky.com/signout/news?successUrl=' + window.location.href);
				},

				viewProfile: function() {
					return false;
				}
			});

