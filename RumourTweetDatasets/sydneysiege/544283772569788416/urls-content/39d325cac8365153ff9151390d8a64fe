		var getCookie = function (sKey) {
				return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
			},

			SSO_token = getCookie('skyCFsidlf01'),
			
			networkConfig = {
				network: 'bskyb.fyre.co'
			},
			
			convConfig = {
				siteId: '360932',
				articleId: '1391976',
