<script type="text/javascript" src="http://www2b.abc.net.au/votecentral/View/js/jsr_class.js"></script>
<script type="text/javascript">     
      function readVoteCentralCookie(cookieName){
				var result = null;
				var cookieNameEquals = cookieName + '=';
				var cookieArray = document.cookie.split(';');
			    
				for (var i = 0; i < cookieArray.length; i++){
					var cookie = cookieArray[i];
			        
					while (cookie.charAt(0) == ' '){
						cookie = cookie.substring(1, cookie.length);
					}
			        
					if (cookie.indexOf(cookieNameEquals) == 0){
						result = cookie.substring(cookieNameEquals.length, cookie.length);
						break;
					}
				}
			    
				return result;
			}
			
			// if user has voted before then set source of iframe to results page
			if(readVoteCentralCookie('ABCElectionRound2338') != null){
				// The web service call
        var req  = 'http://www2b.abc.net.au/votecentral/View/DisplayResult.aspx?e=92&nem=true&rd=2338&callback=getVoteCentralJsonResults'; 
        // Create a new request object
        bObj = new JSONscriptRequest(req); 
        // Build the dynamic script tag
        bObj.buildScriptTag(); 
        // Add the script tag to the page
        bObj.addScriptTag();
			}
			
			function getVoteCentralJsonResults(json){
        var question = "<b>";
        var table = "<table class=\"vc_results_table\" style=\"margin:0!important\">";
        
        question += json.poll.question + "</b>";
        for(var i = 0; i < json.poll.answers.length; i++){
          table += "<tr id=\"rd_2338_"+i+"\">";
          table += "<td class=\"votecentral_candidatecol\">" + json.poll.answers[i].answer + "</td>"; 
          table += "<td class=\"votecentral_resultpercent\">" + json.poll.answers[i].percent + "%</td>";
          table += "<td class=\"votecentral_resultbar\">" +
            "<img src=\"http://www2b.abc.net.au/votecentral/Viewimages/"+(i+1)+".gif\" width=\""+json.poll.answers[i].percent+"px\" height=\"18px\" />&nbsp;" + 
            "</td>";
        }
        table += "<tr><td colspan=\"3\" class=\"vc_results_table_votecount\">"+json.poll.counted+" votes counted</td></tr>";
        document.getElementById("vc_pollcontainer_rd2338").innerHTML = question + table;
      }

    // hide the form and display the results
    function loadVoteCentralResults(IsVoteSubmitted){
      // save the html of the form
      document.getElementById("vc_utility_rd2338").innerHTML = document.getElementById("vc_pollcontainer_rd2338").innerHTML; 
          
      // The web service call
      var req  = 'http://www2b.abc.net.au/votecentral/View/DisplayResult.aspx?e=92&nem=true&rd=2338&callback=getVoteCentralJsonResults'; 
      // Create a new request object
      bObj = new JSONscriptRequest(req); 
      // Build the dynamic script tag
      bObj.buildScriptTag(); 
      // Add the script tag to the page
      bObj.addScriptTag();
              
    	if(!IsVoteSubmitted){
    		document.getElementById("resultsSoFar2338").style.display = "none";
        document.getElementById("backToForm2338").style.display = "block";
    	}
    	else{
        document.getElementById("resultsSoFar2338").style.display = "none";
    		document.getElementById("backToForm2338").style.display = "none";
      }
    
    	return false;
    }
    
    // hide the results and display the selection form
    function showVoteCentralForm2338(){
      document.getElementById("vc_pollcontainer_rd2338").innerHTML = document.getElementById("vc_utility_rd2338").innerHTML;
      document.getElementById("resultsSoFar2338").style.display = "block";
    	document.getElementById("backToForm2338").style.display = "none";
    }
        
    // check that a user has selected at least 1 option
    function validateVoteCentralForm(){
      var foundCheckedOption = false;
    
      // loop through all elements in the form
      for(var i=0;i<document.getElementById("vc_voteform_rd2338").elements.length;i++){
        // find all radio options
        if(document.getElementById("vc_voteform_rd2338").elements[i].type == "radio"){
          foundCheckedOption = document.getElementById("vc_voteform_rd2338").elements[i].checked;
          
          // if we found a checked option then exit loop
          if(foundCheckedOption){
            i = document.getElementById("vc_voteform_rd2338").elements.length;
          }  
        }
      }        
      
      // if no option was selected then display error
      if(!foundCheckedOption){
        document.getElementById("vc_voteform_error_rd_2338").innerHTML = "Error: You must select at least one option to vote for";
        document.getElementById("vc_voteform_error_rd_2338").style.marginTop = "-7px";
        document.getElementById("vc_voteform_error_rd_2338").style.marginBottom = "5px";
        document.getElementById("vc_voteform_error_rd_2338").style.display = "block";
        document.getElementById("vc_voteform_error_rd_2338").style.color = "red";
        document.getElementById("vc_voteform_error_rd_2338").style.fontWeight = "bolder";
      }
      
      return foundCheckedOption;
    }
