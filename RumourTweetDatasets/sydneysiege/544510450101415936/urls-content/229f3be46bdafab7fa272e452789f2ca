    (function () {
        var addScriptToHead = function (url, async, onload) {
            var head = document.getElementsByTagName("head")[0] || document.documentElement;
            var s = document.createElement('script');
            s.type = 'text/javascript';
            if (async) {
                s.async = true;
            }
            if (typeof onload == 'function') {
                // most browsers
                s.onload = onload;
                // IE 6, 7
                s.onreadystatechange = function () {
                    if (this.readyState == 'complete' || this.readyState == 'loaded') {
                        onload();
                    }
                };
            }
            s.src = url;
            head.appendChild(s);
            return s;
        };

        var asyncWidgetsLoad = function () {
            addScriptToHead('http://s7.addthis.com/js/300/addthis_widget.js#pubid=blogtalkradio', true, function () {
                function addthisReady(evt) {
                    addthis.addEventListener('addthis.menu.share', MYBTR.UList.shareComplete);
