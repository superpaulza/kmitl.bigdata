function cleanUp(url) {
    var burl = $.trim(url);
    if (burl.search(/^https?\:\/\//) != -1) burl = burl.match(/^https?\:\/\/([^\/?#]+)(?:[\/?#]|$)/i, "");
    else burl = burl.match(/^([^\/?#]+)(?:[\/?#]|$)/i, "");
    return 'http://'+burl[1]+'/';
}*/
