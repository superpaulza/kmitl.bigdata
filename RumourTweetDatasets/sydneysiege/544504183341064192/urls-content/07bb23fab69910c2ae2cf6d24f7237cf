<!--/Top Images scripts-->
<script type="text/javascript">
    //--- sliderMain (bx-wrapper) js ---
    function isPageEditor() {
        if (typeof Sitecore == "undefined")
            return false;
	    else
		    return true;
	}
	var $j;
	
	if(isPageEditor())
		$j = jQuery.noConflict(true);	//for sitecore preview mode
	else
		$j = jQuery.noConflict();
    
    var viewed = 0;
    $j(document).ready(function(){
        if($j("#sliderBig").length>0){
	
            //The Big Slider
            var realSlider = $j('.bx-wrapper').bxSlider({
                pager:false,
                captions: true,
                speed: 1000,
                nextText:'',
                prevText:'',
                infiniteLoop:false,
                hideControlOnEnd:true, 

                onSlideBefore:function($slideElement, oldIndex, newIndex){
                    changeRealDetailThumb(realThumbSlider,newIndex);
                }
            });

            //The Thumbnails
            var realThumbSlider = $j('.bx-slider').bxSlider({
                minSlides: 2,
                maxSlides: 6,
                slideWidth: 107,
                slideMargin: 16,
                moveSlides: 1,
                pager:false,
                speed:1000,
                infiniteLoop:false,
                hideControlOnEnd:true,
                nextText:'<span></span>',
                prevText:'<span></span>',
	        });
	
            linkRealSliders(realSlider,realThumbSlider);
			
            if ($j(".scrollbox li").length < 6) {
                $j(".scrollbox .bx-prev").hide();
                $j(".scrollbox .bx-next").hide();
            }
	    }
    });

    function linkRealSliders(bigS,thumbS){
	    $(".bx-slider").on("click","a",function(event){
		    event.preventDefault();
		    newIndex=$j(this).parent().attr("slideIndex");
		    bigS.goToSlide(newIndex);
	    });
    }

    function changeRealDetailThumb(slider,newIndex){
        var $thumbS=$j(".scrollbox");
        $thumbS.find('.active').removeClass("active");
        $thumbS.find('li[slideIndex="'+newIndex+'"] a').addClass("active");
        if(slider.getSlideCount()-newIndex>=5)slider.goToSlide(newIndex);
        else slider.goToSlide(slider.getSlideCount()-5);
    
        var $infoS = $j(".info");
        $infoS.find('.active').removeClass("active");
        $infoS.find('li[slideIndex="' + newIndex + '"]').addClass("active");
        
        $(".item_four").show('slide', { direction: 'left' }, 400);
        $('#content_1_storyarticlepage_contente_0_lblCurrentImage').html(parseInt(newIndex)+1);
	    viewed = 1;

        var width = $(window).width();
	    if(width > 767)
		    $('.dim-slide').attr("style", "position:fixed; width:100%; height:100%; top:0; left:0; background:rgba(0,0,0,0.8);z-index: 9999;");
    }

    //--- slide toggle x button js ---
    $ = jQuery.noConflict();
    $("ul.thumbnails li a").click(function () {
        $(".item_four").show('slide', { direction: 'left' }, 400);
	    viewed = 1;
    });
    $(".exit").click(function () {
        $(".item_four").hide('slide', { direction: 'left' }, 400);
	    viewed = 0;
        $('.dim-slide').removeAttr('style');
    });
    $(".dim-slide").click(function () {
        $(".item_four").hide('slide', { direction: 'left' }, 400);
	    viewed = 0;
        $('.dim-slide').removeAttr('style');
    });
    $(".sliderWrap").mouseenter(function () {
        $(".item_four").show('slide', { direction: 'left' }, 400);
    });
    $(".top-wrapper").mouseleave(function () {
	    var width = $(window).width();
	    if(viewed == 0 && width > 767)
            $(".item_four").hide();
	    if(width <= 767)
        {
	        $(".item_four").show();
            $('.dim-slide').removeAttr('style');
        }
    });
</script>
