var y7modules = y7modules || [];
y7modules.push({
    "module" : "async-content",
    config: {
        template_base_path: "https://s.yimg.com/ao/i/mp/multipass-templates/2015032315460/build"
    },
    "init" : function (Y) {
        var AsyncContentLoader = new Y.AsyncContentLoader(this.config);
        AsyncContentLoader.render();
    }
