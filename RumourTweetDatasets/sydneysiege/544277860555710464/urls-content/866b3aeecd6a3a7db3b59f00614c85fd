        'init': function (Y) {
            // make the script tablet only, detect IS touch device
            var isTouchDevice = function () {
                return 'ontouchstart' in window // works on most browsers
                || 'onmsgesturechange' in window; // works on ie10
            };
            if (isTouchDevice()) {
