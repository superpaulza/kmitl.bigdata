	                 //<![CDATA[
					    window.ndm = window.ndm || {};
    window.ndm.gcse = window.ndm.gcse || {
        "cx": '012148326047351459851:6qavvtw72t8',
        "container": '#site-search',
"resultsUrl":"http://www.dailytelegraph.com.au/search-results"
    };
	                 //]]>
		</script>
		<script type='text/javascript'>
	                 //<![CDATA[
					(function($, gcseConfig) {
  $(document).ready(function() {
    var gcse, s,
    cx = gcseConfig.cx || false,
    pre = gcseConfig.pre || false,
    container = gcseConfig.container || '#site-search',
    resultsContainer = gcseConfig.resultsContainer || '.gcse-results',
    isResultPage = $('body.search-results').length && $(resultsContainer).append(gcseResults) || false,
    drawLightForm = gcseConfig.drawLightForm || false,
    gcseResultsUrl = gcseConfig.resultsUrl || '/search-results',
    gcseResults = '<div class="gcse-search"></div>',
    gcseBox = '<div class="gcse-searchbox-only" data-resultsUrl="' + gcseResultsUrl + '"> </div>';
    drawBrandify = true;
    if (drawLightForm) {
      gcseBox = '<form class="media-search-input" action="' + gcseResultsUrl + '"><label><input type="text" name="q" title="search" autocomplete="off" /></label><button type="submit" value="Search">Search</button></form>';
    }
    if (drawBrandify && !isResultPage) {
      gcseBox = '<form id="site-search__form" class="media-search-input gsc-search-box" action="' + gcseResultsUrl + '"><label><input type="text" class="gsc-input" name="q" title="search" autocomplete="off" style="outline: none" /><span class="gsc-search-button" style="position:absolute; right: -25px; bottom: -2px"><input onclick="document.getElementById(\'site-search__form\').submit(); return false;" type="button" value="Search" /></span></label></form>';
    }
    if (isResultPage) {
      $(resultsContainer).append(gcseResults);
    }
    if (pre) {
      $(container).prepend(gcseBox).length;
    } else {
      $(container).append(gcseBox).length;
    }
    // Always load on results page of or light form option it toggled off
    if (cx && ! drawLightForm || ! drawBrandify || isResultPage) {
      gcse = document.createElement('script');
      gcse.type = 'text/javascript';
      gcse.async = true;
      gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//www.google.com/cse/cse.js?cx=' + cx;
      s = document.getElementsByTagName('script')[0];
      s.parentNode.insertBefore(gcse, s);
    }
  });
})(ndm.jQuery || window.jQuery, ndm.gcse || false);
	                 //]]>
		</script>
		<script type='text/javascript'>
	                 //<![CDATA[
					$(document).ready(function() {
	function hotfixSocialSubscribe() {
		if(ndm.social && ndm.social.sitename && $('.social-activity-centre .btn-subscribe').length) {
			var newUrl = '';
			switch(ndm.social.sitename) {
				case "adelaidenow":
					newUrl = "http://www.adelaidenow.com.au/subscribe/news/1/?sourceCode=AAWEB_ONS539";
					break;
				case "dailytelegraph":
					newUrl = "http://www.dailytelegraph.com.au/subscribe/news/1/?sourceCode=DTWEB_ONS539";
					break;
				case "heraldsun":
					newUrl = "http://www.heraldsun.com.au/subscribe/news/1/?sourceCode=HSWEB_ONS539";
					break;
			}
			if(newUrl !== '') {
				$('.social-activity-centre .btn-subscribe').attr('href', newUrl);
			}
		} else {
			setTimeout(hotfixSocialSubscribe, 1000);
		}
	}
	hotfixSocialSubscribe();
});
	                 //]]>
		</script>
