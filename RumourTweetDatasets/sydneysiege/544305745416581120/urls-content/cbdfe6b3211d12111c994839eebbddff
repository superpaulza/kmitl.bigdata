<section id="mediacontentstory" class="yom-mod yom-card-main yom-article" data-uuid="d70c877e-0f41-3c6f-9cf7-f1f0eb54475c" data-type="story" data-ylk="t1:a3;t2:ct-mod;sec:ct-mod;itc:0;rspns:nav;">
<div class="book clearfix">
    <header class="header">
            <h1 class="headline">Sydney hostage crisis: Live Report</h1>
    </header>

    <div class="credit-bar clearfix ">
            <div class="credit-bar-inner">

                <div class="credit">
                                    <a href="http://www.afp.com/" data-ylk="sec:content;slk:provider;itc:0;tar:www.afp.com;ltxt:AFP;"><img src="http://l.yimg.com/os/publish-images/news/2013-08-26/d19448d6-6aaa-4359-a768-eadacf5fbca9_afp-gif_new.gif" alt="AFP" class="provider-img"></a>
