                            if (C2Event.msg == "rmx:no-ad" && C2Event.ad !== "" && adsGlobalObj[C2Event.ad] !== undefined && adsGlobalObj[C2Event.ad].selector !== undefined) {
                                var backfillPos = adsGlobalObj[C2Event.ad],
                                        backfillContainer = YMedia.one(backfillPos.selector);

                                if (!backfillContainer) {
                                    return;
                                }
                                
