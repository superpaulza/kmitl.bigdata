DARLA_CONFIG.events.AUTO = {"name":"AUTO","sp":"7645365","ps":{"FILM":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"ESPON":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"TL1":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"SBTN-1":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"SBTN-0":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"WBTN-1":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"WBTN-0":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"BTN3":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"BTN2":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"BTN1":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"NE3-3":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"NE3-2":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"NE3-1":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"NE3-0":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"WPS":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"LDRB2":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"LREC3":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"LREC2":{"autoMax":25,"autoIV":1,"autoRT":"45000"},"LREC":{"autoMax":25,"autoIV":1,"autoRT":"45000"}},"sa":"refurl=\"refurl_u_afp_com\" Keyword_Id=\"The_Sydney_Morning_Herald;Australia;South_Wales_Police;Sydney;Sheikh_Haron\" \"type\"=\"story\" ctopid=\"1996000;2051500;2054000;18857489;18858989\" ctype=\"fn_news;article\" lang=\"en-US\" rs=\"lmsid:a0ad00000089OcyAAE;revsp:afp.com;lpstaid:d70c877e-0f41-3c6f-9cf7-f1f0eb54475c\" wiki_topics=\"The_Sydney_Morning_Herald;Australia;South_Wales_Police;Sydney;Sheikh_Haron\"  can_suppress_ugc=\"1\" LREC3=\"300x250,1x1\" LREC2=\"300x250,1x1\" LREC=\"300x250,1x1\"","sa_orig":"refurl=\"refurl_u_afp_com\" Keyword_Id=\"The_Sydney_Morning_Herald;Australia;South_Wales_Police;Sydney;Sheikh_Haron\" \"type\"=\"story\" ctopid=\"1996000;2051500;2054000;18857489;18858989\" ctype=\"fn_news;article\" lang=\"en-US\" rs=\"lmsid:a0ad00000089OcyAAE;revsp:afp.com;lpstaid:d70c877e-0f41-3c6f-9cf7-f1f0eb54475c\" wiki_topics=\"The_Sydney_Morning_Herald;Australia;South_Wales_Police;Sydney;Sheikh_Haron\"  can_suppress_ugc=\"1\" LREC3=\"300x250