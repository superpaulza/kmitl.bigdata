                        <script type="text/javascript">
                            $(function () {
                                $('.site-search__form, .search-results__form').submit(function (event) {
                                    var self = this;

                                    var action = $(self).attr('action');
                                    var type = $('input[name="type"]:checked', self);
                                    if (type) {
                                        if (type.val() === 'bing') {
                                            action = 'http://www.bing.com/search';
                                        }
                                    }

                                    var query = $('input:first', self).val();

                                    var location = action + '?q=' + encodeURIComponent(query);
                                    if (type && type.val() === 'bing') {
                                        window.open(location , '_blank');
                                    }
                                    else {
                                        window.open(location, '_self');
                                    }

                                    event.preventDefault();
                                });

                                var searchResultsForm = $('.search-results__form');
                                if (searchResultsForm) {
                                    var searchBox = $('input:first', searchResultsForm);
                                    if (searchBox) {
                                        var queryString = window.location.search;
                                        var queryIndex = queryString.indexOf('q=') + 2;
                                        if (queryIndex > 0) {
                                            queryString = queryString.substring(queryIndex);
                                            searchBox.val(queryString);
                                        }
                                        searchBox.focus();
                                    }
                                }
                            });
                        </script>
