                    <path id="/queensland" d="m 88.644245,1.1692085 c -0.43737,0.40401 -0.65296,0.5 -1.21875,0.5 -0.790619,2.1539961 -2.226774,4.5334762 -1.78125,6 -1.41948,2.4270405 -0.22553,5.5465295 -0.625,7.6874995 -0.850566,3.839512 -3.990229,10.304128 -7.375,7.3125 -0.7895,-1.34181 -2.27733,-2.31655 -3.03125,-2.8125 l -1.0625,30.28125 8.375,0.1875 -0.46875,10.03125 1.03125,0.0937 20.093745,1.6875 c 0.5,-0.6 1.74375,-0.775 2.34375,-0.875 2.52752,0.12294 3.79323,1.38325 4.8125,2.375 0.25769,0.0859 0.58043,-0.0752 0.90625,-0.3125 -1.80739,-0.91139 -3.0625,-2.77514 -3.0625,-4.9375 0,-3.05772 2.47353,-5.53125 5.53125,-5.53125 1.08938,0 2.11225,0.3004 2.96875,0.84375 -0.0406,-0.804082 -0.63291,-1.001599 -1.0625,-1.25 0.5,-1.5 -0.99375,-2.1875 -1.09375,-3.6875 0.1,0.2 -0.30625,0.3875 -0.40625,0.1875 -1,-1.4 -2.2,-2.5 -2.5,-4 -0.14689,-1.622043 0.51352,-3.134273 -1.0937,-3.59375 0.3,0.4 0.21875,0.9125 0.21875,1.3125 -0.38542,-1.024814 -2.7236,-1.907518 -1.5625,-0.34375 -0.1832,-0.21602 -0.6625,-0.025 -0.75,-0.375 -0.84544,-3.823276 -2.0409,-8.748933 -5.5,-10.28125 0,0.1 -0.59375,-0.1 -0.59375,0 -2.609078,-2.706166 -3.404437,-5.619738 -3.406245,-9.3125 -1.49712,-0.92785 -1.73788,-2.47919 -2.1875,-4 0.532967,-2.856308 -0.707315,-3.076449 -2.21875,-5.3125 -0.6,0.4 -1.2875,1.11875 -1.6875,0.71875 C 91.357178,9.5752709 89.259002,4.9442464 88.644295,1.1691585 z" />
                    <path id="/south-australia" d="m 47.182545,51.636558 0.0565,-0.61469 33.20825,0.58073 -2.46146,38.76972 c -0.5,-0.3 -0.35413,-0.21559 -0.85413,-0.61559 -0.5,-0.5 -0.9,-1 -1.3,-1.6 -0.7,-1.1 0.4,-2.3 -0.1,-3.4 -0.2,-0.3 -0.6,-0.5 -0.7,-0.8 -0.2,-1 0.3,-2.6 -0.6,-2.7 -1.2,-0.2 -2,0.9 -3.4,0.6 1.5,-1.2 1.6,-3.2 0.5,-4.8 -0.6,1 -1,2.2 -1.7,3.1 -0.6,0.8 -1.3,0.3 -2,-0.2 0.6,0.2 1,-0.3 1.2,-0.6 0.9,-0.9 0.5,-2.1 1.2,-3.2 0.7,-1.3 1.4,-3.4 0.2,-4.9 -0.1,1.3 -0.7,2.5 -1.4,3.3 -1.3,1.3 -2.8,2.3 -3.8,3.9 -0.2,0.4 0.4,0.7 0.4,1 -0.1,0.5 -1,0.9 -1.1,0.2 -0.2,-0.9 -0.1,-2 -0.5,-2.9 -0.7,-1.5 -1.7,-2.7 -3,-3.8 -0.2,-0.2 0.1,-0.8 0.1,-0.9 -0.4,-1 -1.5,-2.1 -2.5,-1.9 -1.1,0.2 -1.7,-0.7 -2.8,-0.4 -0.5,0.1 -1.4,-0.7 -2.1,-1.1 -2.1,-0.8 -3.85413,-0.52294 -5.85413,0.0771 -0.0616,-2.93245 -0.61875,-14.24298 -0.69507,-17.09727 z" />
