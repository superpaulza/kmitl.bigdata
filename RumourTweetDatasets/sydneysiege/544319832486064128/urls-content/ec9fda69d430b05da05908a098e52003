    <script type="text/javascript">
    $(document).ready(function() {

        if (total_imgs == 1) {
            var aImg = $('.popup-gallery li a');
            
            var text = aImg.attr('title');
            
            $('.field-name-caption').html('<block><p>' + text + '</p></block>');
        }     
        
        var chrMax = 100;
        
        var origText = $('.field-name-caption block p').text();

        var charLength = $('.field-name-caption block p').text().length;
        
        var screen_width = $( window ).width();
        var width_mobile = 650;
        
        var pAbsoluteShow = 0;
        
        if (charLength > chrMax) {
            var shortText = jQuery.trim(origText).substring(0, chrMax).split(" ").slice(0, -1).join(" ") + "... ";
            $('.field-name-caption block p').text(shortText);            
            $('.field-name-caption block p').append('<a id="readMore">Read more</a>');
        }
        

        var screen_width = $( window ).width();

        if (screen_width <= width_mobile) {  
        
        $$('.mfp-figure figure').tap(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {  
                switchPDescription(); 
            }
        }); 

        $$('#p-absolute').unbind('tap').tap(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                      
                switchPDescription(); 
            }
        }); 

        $$('.mfp-figure figure').unbind('swipeLeft').swipeLeft(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                      
                if (mfp.index == (total_imgs - 1)) {
                    mfp.index = 0;
                }
                else {
                    mfp.index = mfp.index + 1;
                }      
                mfp.updateItemHTML();
            }
        });

        $$('.mfp-figure figure').unbind('swipeRight').swipeRight(function() {                
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                   
                if (mfp.index == 0) {
                    mfp.index = total_imgs - 1;
                }
                else {
                    mfp.index = mfp.index -1;
                }
                mfp.updateItemHTML();
            }
        });    
        
        }

       function configMobile() {
            var screen_width = $( window ).width();
            
            if (screen_width <= width_mobile) {        
                showDescription(pAbsoluteShow);        
            }
        }


        $('.popup-gallery').magnificPopup({
            delegate: 'a',
            type: 'image',
            tLoading: 'Loading image #%curr%...',
            mainClass: 'mfp-img-mobile',
            
            gallery: {
              enabled: true,
              navigateByImgClick: true,
              preload: [0,1] // Will preload 0 - before current, and 1 after the current image
            },
            
            image: {
                tError: '<a href="%url%">The image #%curr%</a> could not be loaded.',
                titleSrc: function(item) {
                    return item.el.attr('title');
                }
            },

            callbacks: {
                
                open: function() {

                    $('.mfp-bottom-carrousel').html("");

                    var img_total = 0;

                    $('.popup-gallery li a').each(function(idx,el){

                        img_total = img_total + 1;
                        var _img = document.createElement('img');

                        $(_img).attr('id', 'thumb-gallery-'+idx );
                        $(_img).attr('src', $(el).attr('href'));
                        $(_img).attr('alt', $(el).attr('title'));


                        if (idx == 0) $(_img).addClass('select');

                        $(_img).click(function(){
                            mfp.direction = (idx >= mfp.index);
                            mfp.index = idx;
                            mfp.updateItemHTML();

                            updateCarousel(idx);
                        });

                        if (only_one_photo == 0) {
                            $('.mfp-bottom-carrousel').append(_img);
                        }

                    })

                    var width_img_total = (img_total * 120) + 3;

                    $('.mfp-bottom-carrousel').width(width_img_total + 'px');
                    
                    configMobile();
                },
                close: function() {
                    $('#p-absolute').hide();
                    pAbsoluteShow = 0;
                }
                // e.t.c.
            }
        });
        
        $('#readMore').click(function(){
            $('.open_gallery').click();
            mfp.index = 0;
            mfp.updateItemHTML();
            updateCarousel(0);
        });  
        
        $('#infoGallery').click(function(){
            pAbsoluteShow = 1;
            $('.open_gallery').click();
            mfp.index = 0;
            mfp.updateItemHTML();
            updateCarousel(0);
        });   
        
        $('#button_comments').click(function(){
            if( $('#articleComments').is(":visible") ) {
                $('#articleComments').css('cssText', 'display: none !important');
                $('#button_comments').removeClass('comments_visible');
                $('#button_comments').html('SEE COMMENTS');
            }
            else {
                $('#articleComments').css('cssText', 'display: block !important');
                $('#button_comments').addClass('comments_visible');
                $('#button_comments').html('HIDE COMMENTS');                
            }
        });
        
        
    });
    </script>

 

<div  class="hnews-hidden"><div  class="hnews-author-org">By: <span  class="author vcard"><a href="/content/kristen-gelineau" class="fn">KRISTEN GELINEAU</a></span> (<span  class="source-org vcard"><span  class="org fn">AP</span></span>)</div><span  class="dateline">SYDNEY</span><a  rel="item-license" id="license-63564a41f0066c388be917bf2f4d29f4" href="#license-63564a41f0066c388be917bf2f4d29f4">Copyright 2015 The Associated Press. All rights reserved. This material may not be published, broadcast, rewritten or redistributed.</a><div  class="geo hnews-geo"><span  class="latitude">-33.8679</span><span  class="longitude">151.207</span></div></div><div class="field field-name-body field-type-text-with-summary field-label-hidden entry-content"><div class="field-items"><div class="field-item even"><p><block><p>SYDNEY (AP) — Australian Prime Minister Tony Abbott has laid flowers at a makeshift memorial in Sydney for the victims of a central city cafe siege which left three people dead.</p>
<p>Abbott laid a bouquet of white flowers at the memorial in Martin Place while his wife Margie laid a bouquet of red flowers. Abbott also wrote a message and waved at the crowd but did not address them.</p>
<p>Hundreds of people have been leaving flowers at the memorial.</p>
<p>A gunman took 17 hostages Monday at the Lindt Chocolat Cafe. The siege ended 16 hours later in a barrage of gunfire that left two hostages and the Iranian-born gunman dead, and a nation that has long prided itself on its peace rocked to its core.</p>
</block></p></div></div></div>
</article>



