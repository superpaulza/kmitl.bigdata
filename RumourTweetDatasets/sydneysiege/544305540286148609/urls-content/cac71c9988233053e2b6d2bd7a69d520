        var deps = ['jquery', 'underscore'];

        deps.push(loadSettings ? 'text!/core/users/settings.js/?v=' + (+new Date) : 'settings');
        deps.push('apps/frontpage');

        var editable = false;

        root.require(deps, function($, _, userSettings, app) {
            userSettings = _.isObject(userSettings) ? userSettings : JSON.parse(userSettings);
            $.extend(Settings, userSettings);
            _.each(Settings.sites, function (site) {
                site.id === Settings.site.id && _.extend(Settings.site, site);
            });
            editable = Settings.site ? !!_.find(Settings.site.permissions || [], function (perm) {
                return perm;
            }) : false;
            Settings.user || (Settings.user = {});
            Settings.user.is_autorized = false;
            console.log('Settings', Settings);

            if (Settings.site.is_banned){
                if (Settings.site.is_owner_logged_in){
                    $('#banned_site').show();
                }
                else {
                    root.location.href = "https://www.rebelmouse.com/404/page/not/found";
                }
            }

            app.initialize({
                cols: 0,
                skip: {"": true},
                
                
                
                
                open_power_your_site: 0
            });
        });
    }(this);
    </script>

