Play,vidObj,i,$endSlate,nextVideoPlayTimeout = 1500;if (jQuery.isArray(currentVideoCollection) && currentVideoCollection.length > 0) {for (i = 0; i < currentVideoCollection.length; i++) {vidObj = currentVideoCollection[i];if (typeof vidObj !== 'undefined' && vidObj.videoId && vidObj.videoId === currentVideoId) {if (i < currentVideoCollection.length - 1) {nextPlay = currentVideoCollection[i + 1].videoUrl;} else {nextPlay = currentVideoCollection[0].videoUrl;}break;}}if (typeof nextPlay === 'undefined' || nextPlay === null) {nextPlay = currentVideoCollection[0].videoUrl;}moveToNextTimeout = setTimeout(function () {location.href = nextPlay;}, nextVideoPlayTimeout);} else {$endSlate = jQuery(document.getElementById(containerId)).parent().find('.js-video__end-slate').eq(0);if ($endSlate.length > 0) {videoEndSlateImpl.showEndSlateForContainer();}}}callbackObj = {/** Listen to the metadata event which fires right after the ad ends and the actual video playback begins*/onContentMetadata: function (containerId, playerId, metadata, contentId, duration, width, height) {var endSlateLen = jQuery(document.getElementById(containerId)).parent().find('.js-video__end-slate').eq(0).length;CNN.VideoSourceUtils.updateSource(containerId, metadata);if (endSlateLen > 0) {videoEndSlateImpl.fetchAndShowRecommendedVideos(metadata);}},onAdPlay: function (containerId, cvpId, token, mode, id, duration, blockId, adType) {clearTimeout(moveToNextTimeout);},onContentPlay: function (containerId, cvpId, event) {/** When the video content starts playing, the companion ad* layout (if it was set when the ad played) should switch* back to epic ad layout. onContentPlay calls* updateCompanionLayout with the 'restoreEpicAds' layout to* make this switch*/if (CNN.companion && typeof CNN.companion.updateCompanionLayout === 'function') {CNN.companion.updateCompanionLayout('restoreEpicAds');}clearTimeout(moveToNextTimeout);},onContentBegin: function (containerId, cvpId, contentId) {/** Before the video ad starts,