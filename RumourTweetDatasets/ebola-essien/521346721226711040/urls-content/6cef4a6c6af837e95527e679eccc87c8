var BCL = {};

var onTemplateLoaded = function (experienceID) { BCL.player = brightcove.api.getExperience(experienceID)};
var onTemplateReady = function(evtObj) { BCL.experienceModule = BCL.player.getModule(brightcove.api.modules.APIModules.EXPERIENCE)};

App.resizeBrightcove = function(){
	var prevwidth = $('.BrightcoveExperience').width();
 	var prevheight = $('.BrightcoveExperience').height();
 	if(prevwidth!=$('#singleArticle').width()){
   		$('.BrightcoveExperience').width($('#singleArticle').width()).height((($('#singleArticle').width()/prevwidth)*prevheight));
   		if(BCL.experienceModule){
            BCL.experienceModule.setSize($('#singleArticle').width(),(($('#singleArticle').width()/prevwidth)*prevheight));
        }
   	}
};
(function($) {
    App.resizeBrightcove();
    App.ResponsiveIframe('.story-content iframe');

})(jQuery);
