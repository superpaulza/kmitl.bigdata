            function printResponse(responseObject){
            	if(typeof responseObject !== 'undefined' && typeof responseObject.streamInfo !== 'undefined' && typeof responseObject.streamInfo.commentCount !== 'undefined'){
            		var numberofcomments = responseObject.streamInfo.commentCount;
            		if(numberofcomments!=0){
            			$.ajax({
            				url: '/posts/ajax_updatecommentcount/404751/0/'+numberofcomments,
            			});
            		}
            		$('#gigyacomments header span.main-label').text(numberofcomments+' Comment'+((numberofcomments!=1)?'s':''));
            	}
            }
            gigya.comments.getStreamInfo({categoryID:'News', streamID:'http://www.dailystar.co.uk/news/latest-news/404751/Former-Chelsea-star-Michael-Essien-forced-deny-fake-reports-claiming-he-has-Ebola', callback:printResponse});
			</script>
