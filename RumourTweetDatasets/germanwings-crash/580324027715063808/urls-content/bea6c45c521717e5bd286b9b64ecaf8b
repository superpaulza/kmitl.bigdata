  <script type="text/javascript">
    $(document).on('signInFinished', function(){
      var likeControl = new com.coremedia.rating.HtmlLikeControl('/elastic/social/favourite/news');

      if (ES_USER_COOKIE_INFO.id == undefined) {
        $('.add-to-fav-click-target, .side-new-row .fav-row .link').show().on('click', function() {
          $('.user-info-box .btn-sign-in').trigger('click');
          return false;
        });
      }
      else {
        likeControl.query("1738346", "contentbean:1738346", "contentbean:3032");
        $('.add-to-fav-click-target').on('click', function() {
          likeControl.like("1738346", "contentbean:1738346", "contentbean:3032");
          return false;
        });
      }
    });
  </script>

