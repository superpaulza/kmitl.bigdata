<script language="Javascript" type="text/javascript">
function onClickedForTabCountZero(tabTitle){
	if(tabTitle == 'BUY USED')
	{
		var head = document.getElementsByTagName('head')[0];
		var s = document.createElement('script');
		s.type = 'text/javascript';
		s.text = "Krux('admEvent', 'IgU1GlWa', { });";
		head.appendChild(s);
	}
	
};
function onClickedForTabCountNonZero(tabTitle){
	if(tabTitle == 'SELL A CAR')
	{
		var head = document.getElementsByTagName('head')[0];
		var s = document.createElement('script');
		s.type = 'text/javascript';
		s.text = "Krux('admEvent', 'IgVNad2Z', { });";
		head.appendChild(s);
	}
	else if(tabTitle == 'BUY NEW')
