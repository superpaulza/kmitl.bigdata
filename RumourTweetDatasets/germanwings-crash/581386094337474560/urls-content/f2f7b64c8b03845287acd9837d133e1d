var noOfPuffs = 2;
var lastClass = "";

if(puffs_8120654.length > noOfPuffs) {
    for(var i=0; i < noOfPuffs; i++) {    
    	 var randomIndex =  selectRandomPuffIndex(puffs_8120654);         
         if(i == noOfPuffs-1) { lastClass=" last"; }
         append2LayoutPuffs(8120654, randomIndex, true);
         puffs_8120654.splice(randomIndex, randomIndex+1);          
    }   
} else {
    for(var i=0; i<puffs_8120654.length; i++) {
    	if(i == noOfPuffs-1) { lastClass=" last"; }
        append2LayoutPuffs(8120654, i, true);
    }       
