<script type="text/javascript">
    var firstPostTimer,
        post;

    if (window.performance && window.performance.mark && window.performance.measure) {

        window.performance.mark('start_looking_for_post');

        if (! firstPostTimer) {
            firstPostTimer = window.setInterval(function() {
                post = document.querySelector('.lx-ltc__post-contents');

                if (post) {
                    window.performance.mark('first_post');
                    window.performance.measure('ttfp', 'start_looking_for_post', 'first_post');
                    window.clearInterval(firstPostTimer);
                }
            }, 250);
        }
    }
</script>
