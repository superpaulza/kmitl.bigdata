<script type="text/javascript">
        (function trackingRelatedVideoModule() {
            var $relatedVideoPhotoLink = $('#related-video .photo a');
            var $relatedVideoLink = $('#related-video .related-video-title a');

            function init() {
                $relatedVideoPhotoLink.bind('click', articlePageRelatedVideoTracking);
                $relatedVideoLink.bind('click', articlePageRelatedVideoTracking);
            }

            function articlePageRelatedVideoTracking(e) {
                e.preventDefault();
                var link        = $(this).attr('href');
                dcsMultiTrack(
                        "DCSext.ModClickID", "Article|Related Video|8227",
                        "DCSext.ModID", "Article|Related Video|8227",
                        "DCSext.ModClick", "1",
                        "WT.dl", "1"
