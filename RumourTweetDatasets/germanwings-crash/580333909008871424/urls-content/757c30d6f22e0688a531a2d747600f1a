s.prop31='IV promo list - ' + s.channel + ' ' + type + ' - ' + $(this).find('.title').html().replace(/'/g, "\\'");
s.events='event38';
s.tl(this,'o', 'IV promo - ' + location.hostname);
s.prop30 = s.prop31 = s.events = ''; });
$('#galleriaLoadId-7804719').bind('click', function(e) {
var s = s_gi(s_account);
s.event = 'event8';
});
$('.esi-gallery').click(function(e) {
//console.log('esigallery');
if ($(e.target).hasClass('esi-gallery-rightarrow')) {
var s=s_gi(s_account);
s.linkTrackVars='prop28';
s.prop28=$('.esi-gallery').attr('data-galleryid');//location.href.substr(location.href.lastIndexOf('/') + 1);
//console.log('click28 '+location.href.substr(location.href.lastIndexOf('/') + 1));
}
});
