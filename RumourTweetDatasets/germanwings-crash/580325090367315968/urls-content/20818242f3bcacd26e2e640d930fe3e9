		<script type="text/javascript" src="http://scripts.zeit.de/static/js/zon.js?0120rollback"></script><script type="text/javascript">
					// <![CDATA[
						function get_height(){
							var body = document.body,
								html = document.documentElement;

							var height = Math.max(
								body.scrollHeight, body.offsetHeight,
								html.clientHeight, html.scrollHeight, html.offsetHeight );

							return {"offsetHeight":body.offsetHeight,
									"scrollHeight":body.scrollHeight,
									"clientHeight":html.clientHeight,
									"htmlscrollHeight":html.scrollHeight,
									"htmloffsetHeight":html.offsetHeight,
									"height":height}
						}

						function post_height(){
							if (height <= get_height()){
								height = get_height();
								var msg = {"tablet": is_iPad || is_galaxyTab,
										"height_obj": height,
										"height": height.height};
								window.parent.postMessage(msg, "*");
							}
						}

						if (window.parent != window){
							var height = get_height();
							post_height();
							var observer = new MutationObserver(function(mutations) {
							  mutations.forEach(function(mutation) {
								if (mutation.addedNodes.length>0 &&
								    mutation.addedNodes[0].nodeName == "DIV"){
								  setTimeout('post_height()',1000);
								  }
								});
							});

							var config = { attributes: false, childList: true, characterData: false }
							observer.observe(document.body, config);
						}
					// ]]>
					</script>
