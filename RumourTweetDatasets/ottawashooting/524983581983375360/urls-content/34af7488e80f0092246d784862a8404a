<script>
	var Estadao = {};
	
	Estadao.getCookie = function(c_name){
		var i,x,y,ARRcookies=document.cookie.split(";");
		for (i=0;i<ARRcookies.length;i++){
			x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			x=x.replace(/^\s+|\s+$/g,"");
			if (x==c_name){
				return unescape(y);
			}
		}
	};
	
	Estadao.setCookie = function(c_name,value,exdays){
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString()+";domain=.estadao.com.br");
		document.cookie=c_name + "=" + c_value;
	};
	
	Estadao.Toolbar = {
		service: '/estadao/jfeed/jsonfeedclima.php',
		cidades : {"bahia":{uf: "BA", capital: "Salvador"},"minas_gerais":{uf: "MG", capital: "Belo Horizonte"},"distrito_federal":{uf: "DF", capital: "Brasília"},"rio_grande_do_sul":{uf: "RS", capital: "Porto Alegre"},"rio_de_janeiro":{uf: "RJ", capital: "Rio de Janeiro"},"sao_paulo":{uf: "SP", capital: "São Paulo"}},
		parser_cidade: function(cidade){
			var cidade_ = $('.toolbar .clima .cidade .uf'),
			minima = $('.toolbar .clima .min'),
			maxima = $('.toolbar .clima .max'),
			status = $('.toolbar .clima .ico');
			$.ajax({
				url:Estadao.Toolbar.service,
				dataType:"json",
				async:false,
				data:{cidade: cidade},
				success:function(res){
					if(!res.status) return;
					cidade_.text(res.uf);
					minima.text(res.temperatura[0]+"º");
					maxima.text(res.temperatura[1]+"º");
					status.attr('title', res.status[1]).removeAttr("class").addClass("ico ico"+res.status[0]);
					
					$('.toolbar .clima').removeAttr("style");
				}
			});
		},
		init: function(){
			logado = Estadao.getCookie("PORTAL_AUTH");
			if(typeof logado=="undefined" || logado=="" || logado==null){
				$(".toolbar .assinante .avatar #ad_login").attr({"href":"http://acesso.estadao.com.br/login/?r=" + window.location.href,"title":"Login"});
				}else{
				$(".toolbar .assinante .avatar #ad_login").attr({"href":"http://cadastro.estadao.com.br/logout-ref/" + window.location.href,"title":"Logout"});
				menu_ = '<nav><ul>';
				menu_ +='<li><a href="http://cadastro.estadao.com.br/minha-conta">Minha conta</a></li>';
				menu_ +='<li><a href="http://webmail.estadao.com.br/">Webmail</a></li>';
				menu_ +='<li><a href="http://cadastro.estadao.com.br/newsletter">Newsletter</a></li>';
				menu_ +='<li><a href="http://servicos.estadao.com.br/login/">Edição digital</a></li>';
				menu_ +='<li><a href="http://www.assinante.estadao.com.br/">Central do assinante</a></li>';
				menu_ +='<li><a href="http://acesso.estadao.com.br/login/logout/'+window.location.href+'">Sair</a></li>';
				menu_ +='</ul></nav>';
				$(".toolbar .assinante .avatar").append(menu_);
				fotoUser = Estadao.getCookie("OESP_USR_IMG");
				if(typeof fotoUser && fotoUser != ""){
					$(".toolbar .assinante .avatar img").attr({"src":fotoUser});
				}
			}
			
			qclima = Estadao.getCookie("OESP_LOCATION_NAME");
			qclima = (typeof qclima!="undefined") ? qclima : 'sao_paulo';
			
			Estadao.Toolbar.parser_cidade(qclima);
			
			for(var obj in Estadao.Toolbar.cidades ){
				$(".clima .cidade nav ul").append('<li data-cidade="'+obj+'" data-uf="'+Estadao.Toolbar.cidades[obj]["uf"]+'" title="'+Estadao.Toolbar.cidades[obj]["capital"]+'" >'+Estadao.Toolbar.cidades[obj]["capital"]+'</li>');
			}
			
			$(".clima .cidade nav ul li").click(function(){
				if($(this).data("uf") != $('.toolbar .clima .cidade .uf').text()){
					Estadao.Toolbar.parser_cidade($(this).data("cidade"));
					Estadao.setCookie("OESP_LOCATION_NAME",$(this).data("cidade"),365); 
				}
			});
		}
	};
	
	$(function(){
		Estadao.Toolbar.init();
	});
</script>
