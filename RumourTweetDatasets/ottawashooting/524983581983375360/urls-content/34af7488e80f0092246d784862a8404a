        if ($(".balaoComentario").length > 0) {
            var listaGuids = '';
            var contador = 0;
            
            $(".balaoComentario").each(function(index) {
                
                if (contador > 0){
                    listaGuids += '&';
                }
                
                listaGuids += 'guid[]=' + $(this).data("guid");
                contador++;
            });
            
            $.ajax({
                type: "get",
                dataType: "json",
                url: '/servicos/comentarios/contador/?' + listaGuids,
                success: function(data) {
                    if (data.hasOwnProperty('erro') === false) {
                        var listagem = data.conteudo.listagem;
                        
                        for(i=0; i < listagem.length; i++){
                            $(".balaoComentario[data-guid=" + listagem[i].guid + "]").text(listagem[i].contador);
                        }
                    }
                    
                },
                error: function() {
                     
                }

            });
        }

        
