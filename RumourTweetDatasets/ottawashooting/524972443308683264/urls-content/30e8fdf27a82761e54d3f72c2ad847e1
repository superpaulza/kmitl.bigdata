      <script type="text/javascript">
  var mySlider;
  jQuery(document).ready(function() {
    requiresDependency('http://static.ctvnews.ca/bellmedia/common/js/jquery.bxslider.min.js', function() {
        mySlider = $('#rotator_824598608_7_535350').bxSlider({
                      
                      
                      
                      
                                            minSlides: 1,
                      slideWidth:185,
                      //onSlideNext : function($slideElement, oldIndex, newIndex){
                      //    getCarouselSlide("7.535350", "rotator_824598608_7_535350", newIndex+1);
                          //alert('onSlideNext. oldIndex=[' + oldIndex + '], newIndex=[' + newIndex + '], slideWidth=[3]');              
                      //},
                      //onSlidePrev : function($slideElement, oldIndex, newIndex){
                      //    getCarouselSlide("7.535350", "rotator_824598608_7_535350", newIndex+1);
                          //alert('onSlidePrev. oldIndex=[' + oldIndex + '], newIndex=[' + newIndex + '], slideWidth=[3]');              
                      //},
                      onSlideBefore : function ( $slideElement, oldIndex, newIndex ) {
                        getCarouselSlide7_535350_824598608("7.535350", "rotator_824598608_7_535350", newIndex+1, "7_535350_824598608");
                      },
                      moveSlides: 3,
                      infiniteLoop:false,hideControlOnEnd: true,
                      maxSlides: 3,
                                              prevText: 'Previous'
                                          });
        
        $('#rotator_824598608_7_535350').css("visibility", "visible");
              });
      
      
      // get first slide
      //getCarouselSlide7_535350_824598608("7.535350", "rotator_824598608_7_535350", 1, "7_535350_824598608");
  });
  
  
  function getCarouselSlide7_535350_824598608(path, containingDiv, pageNum, carouselUniqueId) {
  
    if ( loadedPageNum7_535350_824598608.indexOf(pageNum) != -1 ){
      // slide already loaded, no need to do anything
    } else {
      // slide not loaded yet, load it.
        if (path != null) {
          
          //var carouselId = path.replace(/\./g, "_");
          
          var maxItemsPerPage = 3;
          var startIndex = (maxItemsPerPage * pageNum) - maxItemsPerPage;
          var endIndex = startIndex + maxItemsPerPage;
          
          if ( endIndex > 12 ) {
            endIndex = 12;
          }
          
          // need to specify the carousel parent site contentID to correctly render video link with dedicated video page
          var targetPath = "2.625/" + path + "?ot=example.AjaxPageLayout.ot";
          
          if (pageNum != null) {
            targetPath = targetPath + "&pageNum=" + pageNum;
          }
      
          // unique carousel id
          targetPath = targetPath + "&carouselUniqueId=7_535350_824598608";
          
          // teaser image deriv and img width
          targetPath = targetPath + "&imgDeriv=box";
          targetPath = targetPath + "&imgWidth=180";
          targetPath = targetPath + "&teaserHeadlineType=normal";
          
          targetPath = targetPath + "&isAjax=true";
      
          jQuery.ajax({
            url : targetPath,
            success : function(data) {
              
              //alert(data);  
              //alert('done ajax: carouselId=' + carouselId + ', startIndex=' + startIndex + ', endIndex=' + endIndex);
              
              if ( (endIndex - startIndex) < maxItemsPerPage ) {
                 startIndex = endIndex - maxItemsPerPage;
                 if ( startIndex < 0 ) {
                  startIndex = 0;
                 }
              }
              
              for ( k = startIndex; k < endIndex ; k ++ ){
                var carouselIndex = k + 1; // carousel index is 1-based
                //alert('carouselINdex=['+carouselIndex+']');
                //alert($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                
                //$("#carousel" + carouselId + "_"+carouselIndex).html($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                $("#carousel7_535350_824598608_"+carouselIndex).html($(data).find("#carousel7_535350_824598608_"+carouselIndex).html());
                
              }
              
              // flag this page has been loaded
              loadedPageNum7_535350_824598608.push(pageNum);
              
            }
          });
        }
    }
  }
  
  // keep track of which carousel page has been loaded.  If loaded, no need to load again.
  var loadedPageNum7_535350_824598608 = new Array();
  </script>


  
								</div>
                                            



  
  
    
  
					            

    
  
          										                              



  
  
