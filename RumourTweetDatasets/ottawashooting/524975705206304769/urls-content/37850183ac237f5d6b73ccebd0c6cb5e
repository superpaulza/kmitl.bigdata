        var thumbnailUrlPlayer = "http://www.cp24.com/polopoly_fs/1.847474.1340292099!/httpImage/image.jpg_gen/derivatives/landscape_620/image.jpg";
    var thumbnailUrl = "http://www.cp24.com/polopoly_fs/1.847474.1340292099!/httpImage/image.jpg_gen/derivatives/landscape_150/image.jpg";
                            thumbnailUrlPlayer = "http://www.cp24.com/polopoly_fs/1.2066911.1414034168!/httpImage/image.jpg_gen/derivatives/landscape_620/image.jpg";
        thumbnailUrl = "http://www.cp24.com/polopoly_fs/1.2066911.1414034168!/httpImage/image.jpg_gen/derivatives/landscape_150/image.jpg";
        if(isVideoPlayerSupported()){
      addClipToPlaylist(474678, "Pens, Flyers pay special tribute to Ottawa ", "The Pittsburgh Penguins and Philadelphia Flyers paid tribute to Ottawa following a fatal shooting singing \"O Canada\" ahead of their game.   ", thumbnailUrlPlayer, thumbnailUrl);
    }
  </script>
        
                                                      
    
                        <script type="text/javascript">
    jQuery(document).ready(
      function(){
                requiresDependency('http://static.ctvnews.ca/bellmedia/common/js/videoPlayer.axis.js', function() {
          var globalDestination = "cp24_web";
          var globalPlatform = "desktop";
          var clipResourceName = "CP24";
          var videoPlayerBrandId = "CP24";
          var videoPlayerDisplayName = "CP24";
          var videoPlayerAkamaiResource = "CP24";
          var videoPlayerTrackingName = "CP24";
          var videoPlayerDestinationCode = "cp24_web";

          initializeVideoApi(globalDestination, globalPlatform, clipResourceName, videoPlayerBrandId, videoPlayerDisplayName, videoPlayerAkamaiResource, videoPlayerTrackingName, videoPlayerDestinationCode);

          requiresDependency('http://cauth.9c9media.com/auth9/axisAuthApi.js', function() {
        
          setIsDebug(false);

          if(isVideoPlayerSupported()){
            // Initialize player
            jQuery(document).bind('onready.etsapi', function () {
                setPlayerWidth(620);
                setPlayerHeight(348);
                setPlayerVidFolder('620x348/');
                setPlayerLogo('http://www.cp24.com/polopoly_fs/1.844228.1340122789!/httpImage/image.png_gen/derivatives/box_135/image.png');
                setPlayerAdRule("long");
                setPlayerAdZone("default");
				setPlayerAdSite("ctv.cp24");
                setComScoreBranch("cp24");
                setVideoPlayerName("CP24 Video Player Story");
                initPlayer(474678);
            });
          }

          // Send playlist to player when player is ready.
          jQuery(document).bind('isDone.onPlayerReady', function() { setPFUserIP("137.205.117.66");
                                                                     setPFClientSite("www.cp24.com");
                                                                     setPFCanonicalUrl("%2Fcm%2F2.628%2F2.629%2F2.614%2Fsports%2Fpens-flyers-pay-special-tribute-to-ottawa-1.2065778");
                                                                     //setPlaylistTmp(playlist, 0, "http://www.cp24.com/polopoly_fs/1.2066911.1414034168!/httpImage/image.jpg_gen/derivatives/landscape_620/image.jpg"); 
                                                                     });

          // BEGIN CTVOmnitureTracking 
          requiresDependency('http://esi2.ctv.ca/players/mediaplayer/builds/ETS_Universal/includes/Metrics.js', function() {
            Metrics.VIDEOPLAYER_NAME = "CP24 Video Player Story";

                                      requiresDependency('http://player.9c9media.com/channels/cp24_web/apiInterfaceOne.js', function() {});
                      });
          // END CTVOmnitureTracking
                    });
                 });
      });
    </script>
        
              <script type="text/javascript">
  /* ----------------- Event binding ---------------------- */
   // Whenever a video starts, highlight it in the story-video-player nav.
   jQuery(document).unbind('isDone.onVideoStart').bind('isDone.onVideoStart', function(event, clipId, clipTitle, infoObjCategory) { navPageTurner(clipId, infoObjCategory);highlightClip(clipId, 'www'); } );

   requiresDependency('http://static.ctvnews.ca/bellmedia/common/js/articlePlayerNavigation.js', function() {

     // Populate video nav.  Initially load page 1.
     populatePlayerNav(1);
     setCurrClipPage(1);
     setCurrUserPage(1);

     // Populate video nav pagination.  First page's index is 0.
     populatePagination(0);

     // Highlight page 1 by default.
     highlightCurrPageNum(1);

   });
</script>
    

