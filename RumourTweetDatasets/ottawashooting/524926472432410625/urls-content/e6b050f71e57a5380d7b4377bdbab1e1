                      //    getCarouselSlide("7.535213", "rotator_822305941_7_535213", newIndex+1);
                          //alert('onSlidePrev. oldIndex=[' + oldIndex + '], newIndex=[' + newIndex + '], slideWidth=[1]');              
                      //},
                      onSlideBefore : function ( $slideElement, oldIndex, newIndex ) {
                        getCarouselSlide7_535213_822305941("7.535213", "rotator_822305941_7_535213", newIndex+1, "7_535213_822305941");
                      },
                      moveSlides: 1,
                      infiniteLoop:false,hideControlOnEnd: true,
                      maxSlides: 1,
                                              prevText: 'Previous'
                                          });
        
        $('#rotator_822305941_7_535213').css("visibility", "visible");
              });
      
      
      // get first slide
      //getCarouselSlide7_535213_822305941("7.535213", "rotator_822305941_7_535213", 1, "7_535213_822305941");
  });
  
  
  function getCarouselSlide7_535213_822305941(path, containingDiv, pageNum, carouselUniqueId) {
  
    if ( loadedPageNum7_535213_822305941.indexOf(pageNum) != -1 ){
      // slide already loaded, no need to do anything
    } else {
      // slide not loaded yet, load it.
        if (path != null) {
          
          //var carouselId = path.replace(/\./g, "_");
          
          var maxItemsPerPage = 1;
          var startIndex = (maxItemsPerPage * pageNum) - maxItemsPerPage;
          var endIndex = startIndex + maxItemsPerPage;
          
          if ( endIndex > 5 ) {
            endIndex = 5;
          }
          
          // need to specify the carousel parent site contentID to correctly render video link with dedicated video page
          var targetPath = "2.625/" + path + "?ot=example.AjaxPageLayout.ot";
          
          if (pageNum != null) {
            targetPath = targetPath + "&pageNum=" + pageNum;
          }
      
          // unique carousel id
          targetPath = targetPath + "&carouselUniqueId=7_535213_822305941";
          
          // teaser image deriv and img width
          targetPath = targetPath + "&imgDeriv=landscape";
          targetPath = targetPath + "&imgWidth=300";
          targetPath = targetPath + "&teaserHeadlineType=overlay";
          
          targetPath = targetPath + "&isAjax=true";
      
          jQuery.ajax({
            url : targetPath,
            success : function(data) {
              
              //alert(data);  
              //alert('done ajax: carouselId=' + carouselId + ', startIndex=' + startIndex + ', endIndex=' + endIndex);
              
              if ( (endIndex - startIndex) < maxItemsPerPage ) {
                 startIndex = endIndex - maxItemsPerPage;
                 if ( startIndex < 0 ) {
                  startIndex = 0;
                 }
              }
              
              for ( k = startIndex; k < endIndex ; k ++ ){
                var carouselIndex = k + 1; // carousel index is 1-based
                //alert('carouselINdex=['+carouselIndex+']');
                //alert($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                
                //$("#carousel" + carouselId + "_"+carouselIndex).html($(data).find("#carousel" + carouselId + "_"+carouselIndex).html());
                $("#carousel7_535213_822305941_"+carouselIndex).html($(data).find("#carousel7_535213_822305941_"+carouselIndex).html());
                
              }
              
              // flag this page has been loaded
              loadedPageNum7_535213_822305941.push(pageNum);
              
            }
          });
        }
    }
  }
  
  // keep track of which carousel page has been loaded.  If loaded, no need to load again.
  var loadedPageNum7_535213_822305941 = new Array();
