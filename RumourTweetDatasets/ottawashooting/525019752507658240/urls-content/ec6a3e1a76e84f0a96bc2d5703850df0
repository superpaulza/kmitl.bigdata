  <script type="text/javascript">
    window.top.jQuery(window).load(function() {
      var mrec_scrollingAd = window.top.jQuery('#div-gpt-ad-1419380184660-0');
      var mrec_adTop = mrec_scrollingAd.offset().top;
      function fixDiv() {
    	var bottomOffset = 300; // for criteo 900x300
        var mrec_stoppingHeight = window.top.jQuery('#adstop').offset().top - bottomOffset;
        if (window.top.jQuery(window).scrollTop() >= (mrec_stoppingHeight - 650))
          mrec_scrollingAd.css({
            'position': 'relative',
            'top': (mrec_stoppingHeight - mrec_adTop - 650) + 'px'
          });
        else if (window.top.jQuery(window).scrollTop() >= (mrec_adTop - 80))
          mrec_scrollingAd.css({
            'position': 'fixed',
            'top': '80px'
          });
        else
          mrec_scrollingAd.css({
            'position': 'static',
            'top': 'auto'
          });
      }
      window.top.jQuery(window).scroll(fixDiv);
      fixDiv();
    });
  </script>
