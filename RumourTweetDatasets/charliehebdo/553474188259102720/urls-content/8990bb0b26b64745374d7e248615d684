<!--<script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.js"></script>-->
<script src="//www.irishtimes.com/static/js/jquery.validate.min.js"></script>
<script>

jq(document).ready( function() {  

  var visitorid;

  var ca = document.cookie.split(';');
  for(var i=0;i < ca.length;i++) {
    var c = ca[i];
    while (c.charAt(0)==' ') c = c.substring(1,c.length);
    if (c.indexOf('__utma') == 0) {
      visitorid = c.substring('__utma'.length,c.length);
      visitorid = visitorid.split(".")[1];
      break
    }
  }

  if (visitorid != '') {
    jq('#googleid').val( visitorid );
  } 

  jq("#newsdigest-form").validate({
    debug:true,
    rules: {
      name: "required",
      surname: "required",
      email: {
        required: true,
        email: true
      }
    },
    messages: {
      name: {
        required: "Required"
      },
      surname: {
        required: "Required"
      },
      email : {
        required: "Required",
        email: "Not valid"
      }
    },
    submitHandler: function(form) {
      jq.ajax({
        type: "POST",
        url: jq('#newsdigest-form').attr('action'),
        data: jq("#newsdigest-form").serialize(), 
        success: function (data, status) {
          var digestSelection = jq('input[name=digestSelection]:checked', '#newsdigest-form'  ).val();
          _gaq.push([ '_trackEvent', 'Digest Signup', 'digestSelection', digestSelection ]);
        },
        complete: function (data, status) {    
          if (data.status != 200) {
            alerttype="alert-error";
            message = data.responseText;
          } else {
            response = jq.parseJSON(data.responseText);
            if (response.result == 'OK') {
              alerttype="success";
              jq("#newsdigest-form").hide();
              message="<p>Thank you for signing up.</p>";
              message+="<p>You will start to receive Daily Digests from <strong>newsdigest@comms.irishtimes.com</strong>.</p>";
              message+="<p>Please add this email address to your address book.</p>";
            } else {
              alerttype = "alert-error";
              message = response.result;
            }
          }
          jq('#alert').addClass("successOverlay");  
          jq('#alert').html('<div id="alertdiv" class="alert '+alerttype+'"><span>'+message+'</span></div>');
        },             
      });
      return false;
    }
  }); 
});
</script>
