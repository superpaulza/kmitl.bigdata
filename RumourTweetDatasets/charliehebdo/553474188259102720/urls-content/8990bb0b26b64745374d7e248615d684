<script>
jq(document).ready( function($) {
  
  var pw_meter_news = $.cookie('pw_meter_news');
  var it_pw_auth = $.cookie('IT_PW_AUTH');
  if (pw_meter_news && pw_meter_news != ""
    && (!(it_pw_auth || it_pw_auth == ""))
    ) {
    var parts = pw_meter_news.split(".");
  
    var consumed = parseInt( parts[2], 10 );
    
  
   if (consumed == 8
      && jq(document.body).hasClass('isArticle')
      && jq(document.body).hasClass('isMetered')
      && location.href.indexOf('mode=sample') == -1) {
      
      jq.notify({
        message: "You have read " + consumed + " of 10 free articles this week.",
        prompt: "Try unlimited access for just \u20ac1. <span>Find out more</span>"
      }, { 
        globalPosition: 'bottom left',
         //  autoHide: false
        autoHideDelay: 8000
      });
    }
  }
});

jq.notify.addStyle('bootstrap', {
  html: 
    "<div>" +
        "<a href='/digital-subscriptions' class='toast-msg goto-storefront' id='metermsg-storefront'>" +
          "<div class='notify-msg' data-notify-text='message'/>" +
          "<div class='notify-prompt' data-notify-html='prompt'>" +
          "</div>" +
    	"</a>" +
    "</div>"
});



</script>

