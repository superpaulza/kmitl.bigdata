												jq('.latestin .trending a').click(function(e) {		
													_gaq.push(['_trackEvent', 'Latest', (e.currentTarget.href), 'Latest News' ]);
												});	
										});						
									});
								}
							}else{
								return;
							}
						  }						  
						});	
	
			    }, 120000);
			});
