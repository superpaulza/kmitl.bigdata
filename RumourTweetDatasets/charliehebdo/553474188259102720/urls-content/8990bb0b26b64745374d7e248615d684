				var refreshId = setInterval( function() 
			    {
			    	
					var timestamp = new Date().getTime();
					jq.ajax({
						  url: '/editorial-page-teasers/2.1838/span-12-column-splitter-8-4-7.2411545/latest-news-7.2411552?ot=example.AjaxPageLayout.ot&ts='+timestamp,
						  timeout: 10000,
						  
						  complete: function(data, statusMsg){
							if(statusMsg == 'success'){
								var responseText = jq.trim( data.responseText );
								var inpageText = jq.trim( jq('#ajax-placeholder-14718758').text() );
								
								if(responseText != inpageText){
									jq('.spinner').show();
									jq('#ajax-placeholder-14718758' ).animate({
										opacity: 0
										}, 400, 'linear', function() {
											jq('#ajax-placeholder-14718758').html(responseText);
											jq('#ajax-placeholder-14718758' ).animate({
											opacity: 1
											}, 400, 'linear', function() {
												jq('.spinner').hide();
												reduceGap();
