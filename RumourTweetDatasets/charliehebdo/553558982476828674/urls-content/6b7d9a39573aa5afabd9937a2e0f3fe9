<script>
	function getNumInAjaxCall(Id)
	{
		var url = 'http://stats.mako.co.il/ClicksStatistics/getViewsNumber.jsp?contentId='+Id;
		cfAjax.Request(url, function(s){showResponse(s, Id);}, true );
	}

	function showResponse(res,Id){
		if (res != 'failed')
		{
			j$('.statusbar_numOfViews_'+Id).prepend(res);
		}
	}

</script>
