<script type="text/javascript">
	Event.observe(window, 'load', function()
	{
		Lazy.js('rcs@/js/newsTicker.js?v=7',showTicker);
	});
	function showTicker()
	{
		var options = {
    	newsList: "#news",
		tickerRate:50,
		startDelay: 100,
		loopDelay: 3000,
		fadeOutDelay:300,
		placeHolder1: "_",
		placeHolder2: "_",
		controls: true,
		ownControls: true,
		stopOnHover: true
  		}
  		j$("#newsTickerWrapper").css("visibility","visible");
		j$("#newsTickerWrapper").show();
		j$("#news").newsTicker(options);
  	}
</script>
