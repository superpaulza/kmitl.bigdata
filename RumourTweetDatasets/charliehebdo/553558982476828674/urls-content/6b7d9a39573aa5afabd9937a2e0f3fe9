function setKidumLive(){	
	if (isKidumLive)	{		
		if ($('liveTextHref').innerHTML == ''){
			$('liveTextHref').innerHTML = $('news_ordering_title').innerHTML;
		}		
		$('kidumLiveDiv').setStyle({visibility:'visible'});
	}
}
