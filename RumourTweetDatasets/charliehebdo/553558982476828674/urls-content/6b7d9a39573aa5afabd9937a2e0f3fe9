			<script>			
				var loc="standard1";
				if(loc.indexOf("ozen")==-1 || loc.indexOf("ozen_inner")!=-1){
					if(window.DFP && DFP.isStandard)
						googletag.cmd.push(function() { googletag.display('CM8ShowAd_STANDARD1_234X60'); });
					else if (!(DFP.isSpecial && loc.indexOf("plasma")!=-1)){
						var isInterstialOnPage = window.InterstitialObject && window.InterstitialObject.active;
						if((loc.indexOf("peel")==-1 && loc.indexOf("jambo1")==-1 && loc.indexOf("monster1")==-1 && loc.indexOf("poster")==-1) 
						|| (!DFP.isHP && loc.indexOf("jambo1")==-1 && loc.indexOf("poster")==-1)
						|| (loc.indexOf("poster")>-1 && !DFP.isArticle)){
							if (loc.indexOf("ozen_inner") ==-1 && loc.indexOf("monster2") ==-1 && !isInterstialOnPage){										
								googletag.cmd.push(function() { googletag.display('CM8ShowAd_STANDARD1_234X60'); });
							}else{		
								var timeoutDuration = (isInterstialOnPage?(window.InterstitialObject.Duration?(window.InterstitialObject.Duration*1000):3000):0);
								console.info(timeoutDuration +':'+loc );
