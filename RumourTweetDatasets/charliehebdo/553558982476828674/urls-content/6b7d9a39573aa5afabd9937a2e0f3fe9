<script>
	Lazy.js('rcs@/js/jPagerLight.js',function(){
		window.ajaxCallSimulator =  function(ajaxCallUrl, callBackFunction, page, displayTarget, numberOfPages){
			var response = '<ul style="display: none;">';
			response += '<userCallBackFunction>'+ callBackFunction +'</userCallBackFunction>';
			response += '<page>' + page + '</page>';
			response += '<displayTarget>' + displayTarget + '</displayTarget>';
			response += '<numberOfPages>' + numberOfPages + '</numberOfPages>';
			response += '<ajaxCallUrl>' + ajaxCallUrl + '</ajaxCallUrl>';
			response += '</ul>';
			if(j$('#' + ajaxCallUrl + page)[0]){
				response += j$('#'+ajaxCallUrl + page).html();
				}
			myCallBackLight(response);
		}
		
		//@param 'page' - div name prefix + number starting with 1
		window.HomePageMostPopularUserCallBackLight = function(response){
			 j$("#HomePageMostPopular").html(response);
		};
		createPagerLight('HomePageMostPopular', 'HomePageMostPopularUserCallBackLight', 1, 'HomePageMostPopularPagerDiv', 3, "resp");

		window.most_popular_img_hover = function(i){
			var w = 115;
			if(j$.browser.msie){
				if(j$(".quirks").length){
					w = 118;
				}
			}
			j$('#most_popular .num_'+i).css({'border':'1px solid #d1d1d1', 'height':'62', 'width':w});
		};

		window.most_popular_img_out_hover = function(i){
			var w = 115;
			if(j$.browser.msie){
				if(j$(".quirks").length){
					w = 118;
				}
			}
			j$('#most_popular .num_'+i).css({'border':'1px solid #fff', 'height':'62', 'width':w});
		};

		makoFBinit();
	});
</script>
