												function(){setTimeout(function(){googletag.cmd.push(function() {googletag.display('CM8ShowAd_STANDARD1_234X60'); })},timeoutDuration);}
										);});
							}									
						}
						else
							DFP.savedDisplay.push('CM8ShowAd_STANDARD1_234X60');
					}
				}				
