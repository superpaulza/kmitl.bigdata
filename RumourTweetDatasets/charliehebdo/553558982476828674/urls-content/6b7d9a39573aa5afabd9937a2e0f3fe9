		</form-->
		
		<script>
		if(window.controlEnableMap == null)
			window.controlEnableMap = new Object();
			
		window.WEB_SOCKET_SUPPRESS_CROSS_DOMAIN_SWF_ERROR = true;
		window.WEB_SOCKET_SWF_LOCATION = 'http://rcs.mako.co.il/jwebsocket/res/js/flash-bridge/WebSocketMainInsecure.swf';
		
		function enableControl(currPlayerID)
		{
			window.currPlayerID = currPlayerID;
			console.log( "enableControl " + window.currPlayerID);
			window.controlEnableMap[currPlayerID] = true;
		}
		
		function disableControl(currPlayerID)
		{
			window.controlEnableMap[currPlayerID] = false;
		}
		
		function connectWS(barcode, currPlayerID)
		{
//			window.currPlayerID = currPlayerID;
			
			if(window.jWebSocketClient == null || window.barcode != barcode)
			{
				window.barcode = barcode;
				
				console.log("barcode "+window.barcode);
				console.log("currPlayerID " + window.currPlayerID);
				if( jws.browserSupportsWebSockets() ) {
				  window.jWebSocketClient = new jws.jWebSocketJSONClient();
				  // Optionally enable GUI controls here
				} else {
				  // Optionally disable GUI controls here
				  //var lMsg = jws.MSG_WS_NOT_SUPPORTED;
				  //alert( lMsg );
				}
						
				console.log( "Connecting to lURL and logging in as  root ..." );
				var lRes = window.jWebSocketClient.logon('ws://wsnc.mako.co.il/jWebSocket/jWebSocket', 'user', 'user', {
				  // OnOpen callback
				  OnOpen: function( aEvent ) {
					console.log("jWebSocket connection established.");
				  },
				  // OnMessage callback
				  OnMessage: function( aEvent, aToken ) {
					console.log( "jWebSocket '" + aToken.type + "' token received, full message: '" + aEvent.data + "'" + window.jWebSocketClient.fClientId);
					if(aToken.type == 'welcome'){
						window.jWebSocketClient.sendToken({
							ns: "com.keshettv.keshetappinfra.websockets",
							type: "saveBarcodeForConnector",
							barcode: barcode,
							sourceId: window.jWebSocketClient.fClientId
						});				
					}
					if(aToken.type == 'response' && aToken.reqType == 'saveBarcodeForConnector' && aToken.code == 0)
					{
						document.getElementsByName(window.currPlayerID)[0]._controlConnected();
					}
					if(aToken.type == 'event' && window.controlEnableMap[window.currPlayerID]){
						console.log("aToken.action " + aToken.action);
						console.log("aToken.value " + aToken.value);
						console.log("currPlayerID " + window.currPlayerID);
						if(aToken.action == 'play')
						{
							document.getElementsByName(window.currPlayerID)[0]._play();
						}
						if(aToken.action == 'pause')
						{
							document.getElementsByName(window.currPlayerID)[0]._pause();
						}
						if(aToken.action == 'resume')
						{
							document.getElementsByName(window.currPlayerID)[0]._resume();
						}					
						if(aToken.action == 'seek')
						{
							document.getElementsByName(window.currPlayerID)[0]._seek(aToken.value);
						}
						if(aToken.action == 'volume')
						{
							document.getElementsByName(window.currPlayerID)[0]._volume(aToken.value);
						}
						if(aToken.action == 'info')
						{
							document.getElementsByName(window.currPlayerID)[0]._info();
						}
						if(aToken.action == 'seekBack')
						{
							document.getElementsByName(window.currPlayerID)[0]._seekBack();
						}
						if(aToken.action == 'seekForward')
						{
							document.getElementsByName(window.currPlayerID)[0]._seekForward();
						}
						if(aToken.action == 'upVolume')
						{
							document.getElementsByName(window.currPlayerID)[0]._volumeUp();
						}
						if(aToken.action == 'lessVolume')
						{
							document.getElementsByName(window.currPlayerID)[0]._volumeDown();
						}
						if(aToken.action == 'startControl')
						{
							document.getElementsByName(window.currPlayerID)[0]._startControl();
						}
						if(aToken.action == 'clickUp')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickUp();
						}
						if(aToken.action == 'clickDown')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickDown();
						}
						if(aToken.action == 'clickRight')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickRight();
						}
						if(aToken.action == 'clickLeft')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickLeft();
						}
						if(aToken.action == 'clickEnter')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickEnter();
						}
						if(aToken.action == 'clickEsc')
						{
							document.getElementsByName(window.currPlayerID)[0]._clickEsc();
						}				
					}
				  },
				  // OnClose callback
				  OnClose: function( aEvent ) {
					console.log( "jWebSocket connection closed." );
					document.getElementsByName(window.currPlayerID)[0]._controlClosed();
				  }
				});
			}
		}
		</script>
</div>

