function doTaboola() {
  if (typeof __taboola_params == 'undefined') return;
  
  window._taboola = window._taboola || [];
  if (!doTaboola.loaded) {
    _taboola.push({article:'auto'});
    !function (e, f, u) {
      e.async = 1;
      e.src = u;
      f.parentNode.insertBefore(e, f);
    }(document.createElement('script'), document.getElementsByTagName('script')[0], __taboola_params.loader);
    doTaboola.loaded = true;
  }
  
  var cb = function(o){ o.container.removeAttribute('id'); }
  _taboola.push({onrender: cb, mode:__taboola_params.mode, container:__taboola_params.container, placement:__taboola_params.placement, target_type:__taboola_params.target_type});
}
