function KruxUserMatch(res) {
kruxBaseUrl = 'http://beacon.krxd.net/usermatch.gif?';
partnerId = 'gigya';
var src = kruxBaseUrl + 'partner=' + partnerId + '&partner_uid=' + res.UID;
