    define(
        'live-experience/io',
        [
            'live-experience/config', 
            'live-experience/tools/host-selector', 
            'io/app'
        ],
        function (config, HostSelector, IO) {
            var isHybridApp = (config.appSuffix === '.app'),
                isPollingEnabled = true;

            if (Object.keys(config).contains('isCoverageToday')) {
                isPollingEnabled = config.isCoverageToday;
            }

            return new IO({
                host: config.host,
                isUk: config.isUk,
                batch: config.batchRequests,
                environment: HostSelector.getEnvironment(config.host),
                livePush: config.livePush,
                brokerHistory: config.brokerHistory,
                hybridApp: isHybridApp,
                pollingEnabled: isPollingEnabled
            }
        );
    });
    require([
        'jquery-1.9',
        'angular',
        'react',
        'live-experience/angular/factory/io',
